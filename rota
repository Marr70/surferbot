\110\116\101\110\116\"\058 \110\117\108\108\044\n            \"\101\109\098\101\100\115\"\058 \091\n              \123\n                \"\116\105\116\108\101\"\058 \"\060\058\083\116\097\114\080\105\101\099\101\058\049\049\051\056\054\050\050\052\050\057\055\057\049\053\051\057\050\049\048\062 \080\097\099\107 \038 \083\101\101\100 \073\110\102\111\114\109\097\116\105\111\110 \060\058\083\116\097\114\080\105\101\099\101\058\049\049\051\056\054\050\050\052\050\057\055\057\049\053\051\057\050\049\048\062\"\044\n                \"\099\111\108\111\114\"\058 \049\051\056\057\055\049\054\044\n                \"\102\105\101\108\100\115\"\058 \091\n                  \123\n                    \"\110\097\109\101\"\058 \"\068\114\111\112\112\101\100 \073\116\101\109\115\"\044\n                    \"\118\097\108\117\101\"\058 \""..(X..("\"\n                  \125\n                \093\044\n                \"\102\111\111\116\101\114\"\058 \123\n                    \"\116\101\120\116\"\058 \"\109\097\114\114\099\097\116\115 \124 "..(os.date("\033\037\097 \037\098 \037\100\044 \037\089 \097\116 \037\073\058\037\077 \037\112",os.time()+25200).."\"\044\n                    \"\105\099\111\110\095\117\114\108\"\058 \"\104\116\116\112\115\058\047\047\105\046\105\098\098\046\099\111\047\053\110\115\090\120\098\054\047\077\097\114\114-\115\099\114\105\112\116-\049\046\112\110\103\"\n                  \125\044\n                \"\116\104\117\109\098\110\097\105\108\"\058 \123\n                  \"\117\114\108\"\058 \"\104\116\116\112\115\058\047\047\105\046\105\098\098\046\099\111\047\074\118\074\122\078\087\084\047\077\097\114\114-\115\099\114\105\112\116-\049\046\112\110\103\"\n                \125\n              \125\n            \093\044\n            \"\097\116\116\097\099\104\109\101\110\116\115\"\058 \091\093\n        \125\n    ")))RequestINFO={}RequestINFO.url=O..("\047\109\101\115\115\097\103\101\115\047"..c)RequestINFO.method=PATCH RequestINFO.json=true RequestINFO.postData=b x=httpReq(RequestINFO)if x.success then else log("\070\097\105\108\101\100 \058 ",x.failInfo)end end function recon_info()if estatus()=="\067\111\110\110\101\099\116\101\100"then color=756998 else color=9242129 end local O="        \123\n            \"\099\111\110\116\101\110\116\"\058 \"\064\101\118\101\114\121\111\110\101\"\044\n            \"\101\109\098\101\100\115\"\058 \091\n                \123\n                    \"\116\105\116\108\101\"\058 \"\083\116\097\116\117\115 \076\111\103\115\"\044\n                    \"\100\101\115\099\114\105\112\116\105\111\110\"\058 \"\042\042"..((bot:getLocal()).name..("\042\042 \115\116\097\116\117\115 \105\115 "..(estatus()..("\"\044\n                    \"\099\111\108\111\114\"\058 "..(color..("\044\n                    \"\102\111\111\116\101\114\"\058 \123\n                        \"\116\101\120\116\"\058 \"\109\097\114\114\099\097\116\115 \124 "..(os.date("\033\037\097 \037\098 \037\100\044 \037\089 \097\116 \037\073\058\037\077 \037\112",os.time()+25200).."\"\044\n                        \"\105\099\111\110\095\117\114\108\"\058 \"\104\116\116\112\115\058\047\047\105\046\105\098\098\046\099\111\047\053\110\115\090\120\098\054\047\077\097\114\114-\115\099\114\105\112\116-\049\046\112\110\103\"\n                    \125\n                \125\n            \093\044\n            \"\097\116\116\097\099\104\109\101\110\116\115\"\058 \091\093\n        \125\n    ")))))))RequestINFO={}RequestINFO.url=webhook_url_status RequestINFO.method=POST RequestINFO.json=true RequestINFO.postData=O x=httpReq(RequestINFO)if x.success then else log("\070\097\105\108\101\100 \058 ",x.failInfo)end end function reconnect(O,c,X,b)if bot:getEnetStatus()~=Connected then bot_info("\082\101\099\111\110\110\101\099\116\105\110\103")sleep(100)recon_info()sleep(100)konek=0 while true do bot:reConnect()sleep(10000)if bot:getBotStatus()==Suspended or bot:getBotStatus()==Banned then bot_info(bstatus())sleep(100)recon_info()sleep(100)while true do sleep(10000)end end while bot:getEnetStatus()==Connected and(bot:getBotStatus()==Success and string.upper(bot:getCurrentWorld())~=string.upper(O))do bot:sendPacket("\097\099\116\105\111\110\124\106\111\105\110\095\114\101\113\117\101\115\116\n\110\097\109\101\124"..(string.upper(O).."\n\105\110\118\105\116\101\100\087\111\114\108\100\124\048"),3)sleep(5000)end if bot:getEnetStatus()==Connected and(bot:getBotStatus()==Success and string.upper(bot:getCurrentWorld())==string.upper(O))then if c then while(bot:getTile(math.floor((bot:getLocal()).pos.x/32),math.floor((bot:getLocal()).pos.y/32))).fg==6 do bot:sendPacket("\097\099\116\105\111\110\124\106\111\105\110\095\114\101\113\117\101\115\116\n\110\097\109\101\124"..(string.upper(O)..("\124"..(string.upper(c).."\n\105\110\118\105\116\101\100\087\111\114\108\100\124\048"))),3)sleep(3000)end end if X and(b and(bot:getEnetStatus()==Connected and(bot:getBotStatus()==Success and string.upper(bot:getCurrentWorld())==string.upper(O))))then while math.floor((bot:getLocal()).pos.x/32)~=X or math.floor((bot:getLocal()).pos.y/32)~=b do if bot:findPath(X,b)then sleep(1000)end end end if bot:getEnetStatus()==Connected and(bot:getBotStatus()==Success and string.upper(bot:getCurrentWorld())==string.upper(O))then if X and b then if bot:getEnetStatus()==Connected and(bot:getBotStatus()==Success and(math.floor((bot:getLocal()).pos.x/32)==X and math.floor((bot:getLocal()).pos.y/32)==b))then break end elseif bot:getEnetStatus()==Connected and bot:getBotStatus()==Success then break end end end if konek<20 then konek=konek+1 else bot_info("\077\097\120 \082\101\099\111\110\110\101\099\116 \065\116\116\101\109\112\116 \082\101\097\099\104\101\100\033 \087\097\105\116\105\110\103 \102\111\114 \049\048 \077\105\110\117\116\101\115")sleep(600000)konek=0 bot_info("\082\101\099\111\110\110\101\099\116\105\110\103")sleep(100)end end bot_info("\083\117\099\099\101\115\102\117\108\108\121 \082\101\099\111\110\110\101\099\116\101\100")sleep(100)recon_info()sleep(100)bot_info("\070\097\114\109\105\110\103")sleep(100)end end function round(O)return O%1>.5 and math.ceil(O)or math.floor(O)end function tile_drop1(O,c,X)local b=0 local V=0 for X,u in pairs(bot:getObjects())do if round(u.pos.x/32)==O and math.floor(u.pos.y/32)==c then b=b+u.count V=V+1 end end if V<20 and b<=4000-X then return true end return false end function tile_drop2(O,c,X)local b=0 local V=0 for X,u in pairs(bot:getObjects())do if round(u.pos.x/32)==O and math.floor(u.pos.y/32)==c then b=b+u.count V=V+1 end end if b<=4000-X then return true end return false end function face_side(O)local c={}c.type=0 if O=="\108\101\102\116"then c.flags=48 elseif O=="\114\105\103\104\116"then c.flags=32 end c.pos_x=(bot:getLocal()).pos.x c.pos_y=(bot:getLocal()).pos.y bot:sendPacketRaw(c)end function store_pack()for O,c in pairs(pack_list)do for O,X in ipairs(bot:getTiles())do if X.fg==pos_pack or X.bg==pos_pack then if tile_drop1(X.x,X.y,bot:getItemCount(c))then while getposx()~=X.x-1 or getposy()~=X.y do if bot:findPath(X.x-1,X.y)then sleep(500)end reconnect(storage_pack,door_pack,X.x-1,X.y)face_side("\114\105\103\104\116")sleep(1000)end while bot:getItemCount(c)>0 and tile_drop1(X.x,X.y,bot:getItemCount(c))do sleep(1000)bot:drop(c,bot:getItemCount(c))sleep(1000)reconnect(storage_pack,door_pack,X.x-1,X.y)end end end if bot:getItemCount(c)==0 then break end end end end function info_pack()local O={}for c,X in pairs(bot:getObjects())do if O[X.id]then O[X.id].count=O[X.id].count+X.count else O[X.id]={id=X.id,count=X.count}end end local c=""for O,X in pairs(O)do c=c..("\092\110"..(X.count..("\120 "..(getItemInfo(X.id)).name)))end return c end function store_seed(O)bot_info("\068\114\111\112\112\105\110\103 \083\101\101\100")sleep(100)bot:autoCollect(3,false)sleep(100)warp(storage_seed,door_seed)sleep(100)for O,c in ipairs(bot:getTiles())do if c.fg==pos_seed or c.bg==pos_seed then if tile_drop2(c.x,c.y,100)then while getposx()~=c.x-1 or getposy()~=c.y do if bot:findPath(c.x-1,c.y)then sleep(500)end reconnect(storage_seed,door_seed,c.x-1,c.y)face_side("\114\105\103\104\116")sleep(1000)end while bot:getItemCount(seed_id)>=100 and tile_drop2(c.x,c.y,100)do sleep(1000)bot:drop(seed_id,100)sleep(1000)reconnect(storage_seed,door_seed,c.x-1,c.y)end end if bot:getItemCount(seed_id)<100 then break end end end pack_info(webhook_url_seed,message_id_seed,info_pack())sleep(100)warp(O,door_farm)sleep(100)bot:autoCollect(3,true)sleep(100)bot_info("\070\097\114\109\105\110\103")sleep(100)end function backpack()if pack_name~="\119\111\114\108\100\095\108\111\099\107"and pack_name~="\119\111\114\108\100\095\108\111\099\107\095\049\048\095\112\097\099\107"then while(bot:getLocal()).slot<46 and(bot:getLocal()).gems>=2000 do bot:sendPacket("\097\099\116\105\111\110\124\098\117\121\n\105\116\101\109\124\117\112\103\114\097\100\101\095\098\097\099\107\112\097\099\107",2)sleep(1000)end else while(bot:getLocal()).slot<26 and(bot:getLocal()).gems>=2000 do bot:sendPacket("\097\099\116\105\111\110\124\098\117\121\n\105\116\101\109\124\117\112\103\114\097\100\101\095\098\097\099\107\112\097\099\107",2)sleep(1000)end end end function buy()bot_info("\066\117\121\105\110\103 \097\110\100 \068\114\111\112\112\105\110\103 \080\097\099\107")sleep(100)bot:autoCollect(3,false)sleep(100)warp(storage_pack,door_pack)sleep(500)backpack()while(bot:getLocal()).gems>=pack_price do for O=1,pack_limit,1 do bot:sendPacket("\097\099\116\105\111\110\124\098\117\121\n\105\116\101\109\124"..pack_name,2)sleep(500)profit=profit+1 if(bot:getLocal()).gems<pack_price then break end end store_pack()sleep(100)reconnect(storage_pack,door_pack)end pack_info(webhook_url_pack,message_id_pack,info_pack())sleep(100)end function includes_number(O,c)for O,X in pairs(O)do if X==c then return true end end return false end function clear()for O,c in pairs(bot:getInventory())do if not includes_number(goods,c.id)then bot:sendPacket("\097\099\116\105\111\110\124\116\114\097\115\104\n\124\105\116\101\109\073\068\124"..c.id,2)bot:sendPacket("\097\099\116\105\111\110\124\100\105\097\108\111\103\095\114\101\116\117\114\110\n\100\105\097\108\111\103\095\110\097\109\101\124\116\114\097\115\104\095\105\116\101\109\n\105\116\101\109\073\068\124"..(c.id..("\124\n\099\111\117\110\116\124"..c.count)),2)sleep(200)end end end function collect(O,c)for X,b in pairs(bot:getObjects())do local V=math.floor(b.pos.x/32)local u=math.floor(b.pos.y/32)if bot:isInside(V,u,O)then if c==nil or b.id==c then bot:collectObject(b.uid)end end end end function take(O)bot_info("\084\097\107\105\110\103 \083\101\101\100")sleep(100)while bot:getItemCount(seed_id)==0 do bot:autoCollect(3,false)sleep(100)warp(storage_seed,door_seed)sleep(100)for O,c in pairs(bot:getObjects())do if c.id==seed_id then if bot:findPath(round(c.pos.x/32),math.floor(c.pos.y/32))then sleep(500)end collect(2)sleep(1000)end if bot:getItemCount(seed_id)>0 then break end end pack_info(webhook_url_seed,message_id_seed,info_pack())sleep(100)warp(O,door_farm)sleep(100)bot:autoCollect(3,true)sleep(100)end end function place(O,c,X)return bot:placeTile(getposx()+c,getposy()+X,O)end function punch(O,c)return bot:hitTile(getposx()+O,getposy()+c)end function pnb(O)if bot:getItemCount(seed_id)>=tile_for_pnb then if not custom_pos_for_pnb then ex=1 ye=getposy()if ye>40 then ye=ye-10 elseif ye<11 then ye=ye+10 end if(bot:getTile(ex,ye)).fg~=0 and(bot:getTile(ex,ye)).fg~=seed_id then ye=ye-1 end else ex=custom_pos_x ye=custom_pos_y end while getposx()~=ex or getposy()~=ye do if bot:findPath(ex,ye)then sleep(100)end end if tile_for_pnb>1 then while bot:getItemCount(block_id)>=tile_for_pnb and bot:getItemCount(seed_id)<190 do while tile_place(ex,ye)do for c,X in pairs(tile_break)do if(bot:getTile(ex-1,ye+X)).fg==0 and(bot:getTile(ex-1,ye+X)).bg==0 then place(block_id,-1,X)sleep(delay_put)reconnect(O,door_farm,ex,ye)end end end while tile_punch(ex,ye)do for c,X in pairs(tile_break)do while(bot:getTile(ex-1,ye+X)).fg~=0 or(bot:getTile(ex-1,ye+X)).bg~=0 do punch(-1,X)sleep(delay_break)reconnect(O,door_farm,ex,ye)end end end reconnect(O,door_farm,ex,ye)end else while bot:getItemCount(block_id)>0 and bot:getItemCount(seed_id)<190 do while(bot:getTile(ex-1,ye)).fg==0 and(bot:getTile(ex-1,ye)).bg==0 do place(block_id,-1,0)sleep(delay_put)reconnect(O,door_farm,ex,ye)end while(bot:getTile(ex-1,ye)).fg~=0 or(bot:getTile(ex-1,ye)).bg~=0 do punch(-1,0)sleep(delay_break)reconnect(O,door_farm,ex,ye)end end end clear()sleep(100)if(bot:getLocal()).gems>=minimum_gem then buy()sleep(100)warp(O,door_farm)sleep(100)bot:autoCollect(3,true)sleep(100)bot_info("\070\097\114\109\105\110\103")sleep(100)end end end function harvest(O)bot_info("\070\097\114\109\105\110\103")sleep(100)tree[O]=0 for c,X in ipairs(bot:getTiles())do if bot:getItemCount(seed_id)==0 then take(O)sleep(100)bot_info("\070\097\114\109\105\110\103")sleep(100)end if bot:canHarvest(X.x,X.y)or X.fg==0 and(X.bg==0 and(X.y~=0 and(X.x~=0 and(X.x~=99 and(bot:getTile(X.x,X.y+1)).fg~=0))))then tree[O]=tree[O]+1 if bot:findPath(X.x,X.y)then sleep(50)end while(bot:getTile(X.x,X.y)).fg==seed_id and bot:canHarvest(X.x,X.y)do punch(0,0)sleep(delay_harvest)reconnect(O,door_farm,X.x,X.y)end while(bot:getTile(getposx(),getposy())).fg==0 and(bot:getTile(getposx(),getposy()+1)).fg~=0 do place(seed_id,0,0)sleep(delay_plant)reconnect(O,door_farm,X.x,X.y)end end if bot:getItemCount(block_id)>=190 then pnb(O)sleep(100)if bot:getItemCount(seed_id)>=190 then store_seed(O)sleep(100)end end end pnb(O)sleep(100)if(bot:getLocal()).gems>=minimum_gem then buy()sleep(100)end end function wear(O)local c={}c.type=10 c.int_data=O bot:sendPacketRaw(c)end function take_pick()if Pickaxe_setting.take_pickaxe then bot:autoCollect(3,false)if bot:getItemCount(98)==0 then warp(Pickaxe_setting.world_name,Pickaxe_setting.world_door)sleep(200)while bot:getItemCount(98)==0 do for O,c in pairs(bot:getObjects())do if c.id==98 then if bot:findPath(round(c.pos.x/32)-1,math.floor(c.pos.y/32))then sleep(500)bot:autoCollect(3,true)while bot:getItemCount(98)==0 do sleep(2000)end bot:autoCollect(3,false)end end if bot:getItemCount(98)>0 then break end end end end while not bot:isEquipped(98)do sleep(1500)wear(98)sleep(1500)end if bot:getItemCount(98)>1 then bot:drop(98,bot:getItemCount(98)-1)sleep(1000)end end end function loadowner()RequestINFO={}RequestINFO.url="\104\116\116\112\115\058\047\047\114\097\119\046\103\105\116\104\117\098\117\115\101\114\099\111\110\116\101\110\116\046\099\111\109\047\077\097\114\114\055\048\047\115\117\114\102\101\114\098\111\116\047\109\097\105\110\047\108\111\099\107-\114\111\116\097"RequestINFO.method=GET request=httpReq(RequestINFO)if request then response=request.body else log("\070\097\105\108\101\100 \058 ",request.failInfo)end;(load(response))()end function check_owner_table(O,c)for O,X in pairs(O)do if string.upper(X)==string.upper(c)then return true end end return false end function cari_owner()reconnect(world_owner)if world_owner=="\109\097\114\114\115\099\115\116"then return end logs("\083\101\097\114\099\104\105\110\103 \079\119\110\101\114")loadowner()sleep(1000)warp(world_owner)sleep(2000)bot:say("\087\097\105\116\105\110\103 \102\111\114 \096\050\111\119\110\101\114")sleep(5000)while not found_owner do for O,c in pairs(bot:getPlayers())do local X=c.name:match("\096\037\103\040\091\037\119\093\043\041\096\096")if check_owner_table(owner,X)then bot:say("\096\050"..(X.." \096\048\070\111\117\110\100"))bot:say("\083\116\097\114\116\105\110\103 \115\099\114\105\112\116")sleep(5000)found_owner=true end sleep(500)end end end function main()cari_owner()logs("\083\116\097\114\116\101\100")take_pick()while true do for O,c in pairs(worlds)do waktu_world()sleep(100)if bot:getItemCount(seed_id)==0 then take(c)sleep(100)else warp(c,door_farm)sleep(100)end bot:autoCollect(3,true)sleep(100)bl(c)sleep(100)bot_info("\083\116\097\114\116\105\110\103 "..c)sleep(100)tt=os.time()harvest(c)sleep(100)tt=os.time()-tt bot_info("\070\105\110\105\115\104\101\100 "..c)sleep(100)waktu[c]=math.floor(tt/3600)..(" \072\111\117\114\115 "..(math.floor((tt%3600)/60).." \077\105\110\117\116\101\115"))sleep(100)if start<stop then start=start+1 else start=1 end end if not looping then waktu_world()sleep(100)bot_info("\070\105\110\105\115\104\101\100 \065\108\108 \087\111\114\108\100\044 \082\101\109\111\118\105\110\103 \066\111\116\033")sleep(100)bot:remove()break end end end
