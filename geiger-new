x()]=I end end return R end function punch(I,R)local D=bot:getLocal()return bot:hitTile(math.floor(D.pos.x/32)+I,math.floor(D.pos.y/32)+R)end function break_toxic_waste()waste_tiles={}for I,R in ipairs(bot:getTiles())do if R.fg==778 or R.bg==778 then table.insert(waste_tiles,{x=R.x;y=R.y})end end if#waste_tiles>0 then logs("\067\108\101\097\114\105\110\103 \084\111\120\105\099 \087\097\115\116\101")while#waste_tiles>0 do for I,R in pairs(waste_tiles)do if bot:findPath(R.x,R.y+1)then sleep(500)punch(0,-1)sleep(1000)table.remove(waste_tiles,I)elseif bot:findPath(R.x,R.y-1)then sleep(500)punch(0,1)sleep(1000)table.remove(waste_tiles,I)elseif bot:findPath(R.x+1,R.y)then sleep(500)punch(-1,0)sleep(1000)table.remove(waste_tiles,I)elseif bot:findPath(R.x-1,R.y)then sleep(500)punch(1,0)sleep(1000)table.remove(waste_tiles,I)end end end logs("\068\111\110\101 \067\108\101\097\114\105\110\103 \084\111\120\105\099 \087\097\115\116\101")end end function bstatus()return bot:getBotStatus()end function estatus()return bot:getEnetStatus()end function cek_multibot(I,R)for I,D in pairs(I)do if string.upper(D.name)==string.upper(R)then return true end end return false end function main()midX,midY=50,27 goods={}check_geiger()if stop_script then return end if charging then return end tele(geiger_world,geiger_id)if multibot then for I,R in pairs(duplicate_line)do if not(getBot(R)):isEquipped(2204)then while(getBot(R)):getItemCount(2204)==0 and(getBot(R)):getItemCount(2286)==0 do sleep(2000)end end end for I,R in pairs(duplicate_line)do if string.upper(R)==string.upper(((getBot()):getLocal()).name)then break end if string.upper(R)~=string.upper(((getBot()):getLocal()).name)then while(getBot(R)):getItemCount(2286)==0 do sleep(2000)if(getBot(R)):getEnetStatus()~=Connected and(getBot(R)):getBotStatus()~=Success then break end end end end end sleep(2000)break_toxic_waste()reserve_items()blup=false bldown=false blright=false blleft=false bltopright=false blbottomright=false blbottomleft=false b