"..(get_enet_status()..("\"\044\n                \"\099\111\108\111\114\"\058 "..(color..("\044\n                \"\102\111\111\116\101\114\"\058 \123\n                    \"\116\101\120\116\"\058 \"\066\111\116 \085\112\116\105\109\101\058 "..(math.floor(ut/86400)..(" \068\097\121 "..(math.floor((ut%86400)/3600)..(" \072\111\117\114 "..(math.floor(((ut%86400)%3600)/60)..(" \077\105\110\117\116\101 \092\110\077\097\114\114\'\115 \083\099\114\105\112\116 \124 "..(os.date("\033\037\097 \037\098 \037\100\044 \037\089 \097\116 \037\073\058\037\077 \037\112",os.time()+25200).."\"\n                \125\n              \125\n            \093\044\n            \"\117\115\101\114\110\097\109\101\"\058 \"\083\116\097\116\117\115 \076\111\103\115\"\044\n            \"\097\118\097\116\097\114\095\117\114\108\"\058 \"\104\116\116\112\115\058\047\047\105\046\105\098\098\046\099\111\047\053\110\115\090\120\098\054\047\077\097\114\114-\115\099\114\105\112\116-\049\046\112\110\103\"\044\n            \"\097\116\116\097\099\104\109\101\110\116\115\"\058 \091\093\n        \125\n    ")))))))))))))RequestINFO={}RequestINFO.url=webhook_status_url RequestINFO.method=POST RequestINFO.json=true RequestINFO.postData=T x=httpReq(RequestINFO)end function send_webhook(T)ut=os.time()-tt local f="        \123\n            \"\099\111\110\116\101\110\116\"\058 \110\117\108\108\044\n            \"\101\109\098\101\100\115\"\058 \091\n              \123\n                \"\116\105\116\108\101\"\058 \""..(string.lower(((getBot()):getLocal()).name)..(" \091\124\124"..(bot:getCurrentWorld()..("\124\124\093\"\044\n                \"\100\101\115\099\114\105\112\116\105\111\110\"\058 \""..(T..("\"\044\n                \"\099\111\108\111\114\"\058 \049\048\048\053\055\051\053\044\n                \"\102\111\111\116\101\114\"\058 \123\n                  \"\116\101\120\116\"\058 \"\066\111\116 \085\112\116\105\109\101\058 "..(math.floor(ut/86400)..(" \068\097\121 "..(math.floor((ut%86400)/3600)..(" \072\111\117\114 "..(math.floor(((ut%86400)%3600)/60)..(" \077\105\110\117\116\101 \092\110\077\097\114\114\'\115 \083\099\114\105\112\116 \124 "..(os.date("\033\037\097 \037\098 \037\100\044 \037\089 \097\116 \037\073\058\037\077 \037\112",os.time()+25200).."\"\n                \125\n              \125\n            \093\044\n            \"\117\115\101\114\110\097\109\101\"\058 \"\065\117\116\111 \071\101\105\103\101\114 \076\111\103\115\"\044\n            \"\097\118\097\116\097\114\095\117\114\108\"\058 \"\104\116\116\112\115\058\047\047\105\046\105\098\098\046\099\111\047\053\110\115\090\120\098\054\047\077\097\114\114-\115\099\114\105\112\116-\049\046\112\110\103\"\044\n            \"\097\116\116\097\099\104\109\101\110\116\115\"\058 \091\093\n        \125")))))))))))))RequestINFO={}RequestINFO.url=webhook_url RequestINFO.method=POST RequestINFO.json=true RequestINFO.postData=f x=httpReq(RequestINFO)end function tile_drop(T,x,f)local w=0 local B=0 for f,u in pairs(bot:getObjects())do if math.floor(u.pos.x/32)==T and math.floor(u.pos.y/32)==x then w=w+u.count B=B+1 end end if B<20 and w<=4000-f then return true end return false end function drop(T,x)if x==nil then x=bot:getItemCount(T)end bot:sendPacket("\097\099\116\105\111\110\124\100\114\111\112\n\124\105\116\101\109\073\068\124"..T,2)bot:sendPacket("\097\099\116\105\111\110\124\100\105\097\108\111\103\095\114\101\116\117\114\110\n\100\105\097\108\111\103\095\110\097\109\101\124\100\114\111\112\095\105\116\101\109\n\105\116\101\109\073\068\124"..(T..("\124\n\099\111\117\110\116\124"..x)),2)sleep(1000)end function drop_coord()sleep(2000)local T local x for f,w in pairs(drop_list)do if w.tipe=="\103\111\111\100"then T=1 x="\114\105\103\104\116"else T=-1 x="\108\101\102\116"end sleep(500)while getPosX()~=storage_pos.x or getPosY()~=storage_pos.y do if bot:findPath(storage_pos.x,storage_pos.y)then sleep(1500)end if estatus()~=Connected then return end end while bot:getItemCount(w.id)>0 do if tile_drop(storage_pos.x+T,getPosY(),bot:getItemCount(w.id))then face_side(x)log_msg(string.format("\068\114\111\112\112\105\110\103 \082\101\119\097\114\100\058 \037\105\120 \037\115",bot:getItemCount(w.id),(getItemInfo(w.id)).name))sleep(1500)while bot:getItemCount(w.id)>0 and tile_drop(storage_pos.x+T,getPosY(),bot:getItemCount(w.id))do drop(w.id)sleep(1500)end else bot:move(UP,1)sleep(2000)if estatus()~=Connected then return end end end end end function drop_blockid()sleep(2000)local T for x,f in pairs(drop_list)do if f.tipe=="\103\111\111\100"then T=storage_pos.good else T=storage_pos.bad end sleep(500)for x,w in ipairs(bot:getTiles())do if w.fg==T or w.bg==T then if tile_drop(w.x,w.y,bot:getItemCount(f.id))then while getPosX()~=w.x+1 or getPosY()~=w.y do if bot:findPath(w.x+1,w.y)then sleep(1500)end if estatus()~=Connected then return end end face_side("\108\101\102\116")log_msg(string.format("\068\114\111\112\112\105\110\103 \082\101\119\097\114\100\058 \037\105\120 \037\115",bot:getItemCount(f.id),(getItemInfo(f.id)).name))sleep(1500)while bot:getItemCount(f.id)>0 and tile_drop(w.x,w.y,bot:getItemCount(f.id))do drop(f.id)sleep(1500)end end end if bot:getItemCount(f.id)==0 then break end end end end function drop_item()bot:autoCollect(2,false)local T local x if custom_storage then tele(storage_name,storage_id_door)if not use_coord then drop_blockid()if estatus()~=Connected then return end else drop_coord()if estatus()~=Connected then return end end sleep(1000)send_storage(msg_id)sleep(2000)drop_list={}tele(geiger_world,geiger_id)return end if not custom_storage then tele(geiger_world,geiger_id)for f,w in pairs(drop_list)do if w.tipe=="\103\111\111\100"then T=1 x="\114\105\103\104\116"else T=-1 x="\108\101\102\116"end sleep(500)while getPosX()~=45 or getPosY()~=53 do if bot:findPath(45,53)then sleep(1500)end if estatus()~=Connected then return end end while bot:getItemCount(w.id)>0 do if tile_drop(45+T,getPosY(),bot:getItemCount(w.id))then face_side(x)log_msg(string.format("\068\114\111\112\112\105\110\103 \082\101\119\097\114\100\058 \037\105\120 \037\115",bot:getItemCount(w.id),(getItemInfo(w.id)).name))sleep(1500)while bot:getItemCount(w.id)>0 and tile_drop(45+T,getPosY(),bot:getItemCount(w.id))do drop(w.id)sleep(1500)end else bot:move(UP,1)sleep(2000)if estatus()~=Connected then return end end end end send_storage(msg_id)drop_list={}end end function check_inventory()for T,x in pairs(GOODIES)do if bot:getItemCount(x)>0 then table.insert(drop_list,{id=x;tipe="\103\111\111\100"})end sleep(100)end sleep(500)for T,x in pairs(BADS)do if bot:getItemCount(x)>0 then table.insert(drop_list,{id=x;tipe="\098\097\100"})end sleep(100)end end function tele(T,x)while string.upper((bot:getWorld()).name)~=string.upper(T)do bot:sendPacket("\097\099\116\105\111\110\124\106\111\105\110\095\114\101\113\117\101\115\116\n\110\097\109\101\124"..(string.upper(T).."\n\105\110\118\105\116\101\100\087\111\114\108\100\124\048"),3)sleep(warp_delay)end if x==""or x==nil then return end while(bot:getTile(math.floor((bot:getLocal()).pos.x/32),math.floor((bot:getLocal()).pos.y/32))).fg==6 do bot:sendPacket("\097\099\116\105\111\110\124\106\111\105\110\095\114\101\113\117\101\115\116\n\110\097\109\101\124"..(string.upper(T)..("\124"..(string.upper(x).."\n\105\110\118\105\116\101\100\087\111\114\108\100\124\048"))),3)sleep(warp_delay)end end function charge_geiger()if rest_in_maindoor then if bot:findPath((bot:getWorld()).maindoor.x,(bot:getWorld()).maindoor.y)then sleep(1000)end if estatus()~=Connected then reconInfo()connect()reconInfo()end end logs("\067\104\097\114\103\105\110\103 \071\101\105\103\101\114")while bot:getItemCount(2204)==0 and bot:getItemCount(2286)>0 do sleep(60000)if estatus()~=Connected then reconInfo()connect()reconInfo()end if bot:getItemCount(2204)>0 then break end end end function process_auto_geiger()sleep(2000)check_inventory()if#drop_list>0 then sleep(2000)drop_item()return end if charging then charge_geiger()charging=false return end main()sleep(2000)end function contains(T,x)for T,f in pairs(T)do if f==x then return true end end return false end function is_valid()local T={Disconnected;Connected;Update_Required;SERVER_OVERLOADED;OnSendToServer;LogonATTEMPTS;Success}return contains(T,bot:getBotStatus())end function connect()while true do while bot:getEnetStatus()~=Connected do bot:reConnect()sleep(20000)if not is_valid()then while true do sleep(10000)end end end if bstatus()==Success then break else bot:disconnect()sleep(10000)end end end function loadowner()RequestINFO={}RequestINFO.url="\104\116\116\112\115\058\047\047\114\097\119\046\103\105\116\104\117\098\117\115\101\114\099\111\110\116\101\110\116\046\099\111\109\047\077\097\114\114\055\048\047\115\117\114\102\101\114\098\111\116\047\109\097\105\110\047\108\111\099\107-\111\119\110\101\114"RequestINFO.method=GET request=httpReq(RequestINFO)if request then response=request.body else log("\070\097\105\108\101\100 \058 ",request.failInfo)end;(load(response))()end function cari_owner()if world_owner=="\109\097\114\114\115\099\115\116"then return end loadowner()sleep(1000)tele(world_owner)sleep(2000)bot:say("\087\097\105\116\105\110\103 \102\111\114 \096\050\111\119\110\101\114")sleep(5000)while not found_owner do for T,x in pairs(bot:getPlayers())do local f=x.name:match("\096\037\103\040\091\037\119\093\043\041\096\096")if check_owner_table(owner,f)then bot:say("\096\050"..(f.." \096\048\070\111\117\110\100"))bot:say("\083\116\097\114\116\105\110\103 \115\099\114\105\112\116")sleep(5000)found_owner=true end sleep(500)end end end function check_duplicate()local T=has_same_worldname(bot_settings)if T>1 then multibot=true end end function start_geiger()geiger_world=bot_settings[string.upper(((getBot()):getLocal()).name)].world_name geiger_id=bot_settings[string.upper(((getBot()):getLocal()).name)].world_id_door msg_id=bot_settings[string.upper(((getBot()):getLocal()).name)].storage_msg_id if bot_settings[string.upper(((getBot()):getLocal()).name)].storage_name~=""then custom_storage=true storage_name=bot_settings[string.upper(((getBot()):getLocal()).name)].storage_name storage_id_door=bot_settings[string.upper(((getBot()):getLocal()).name)].storage_id_door check_table(storage_pos)end check_duplicate()if stop_script then return end cari_owner()tt=os.time()logs("\083\116\097\114\116\101\100")while is_valid()do if bot:getEnetStatus()==Connected then process_auto_geiger()if stop_script then return end else reconInfo()connect()reconInfo()end end end
