bot=getBot()worlds={}tile_break={}tree={}waktu={}fossil={}str_waktu=""profit=0 list_now=1 t=os.time()found_owner=false storage_seed=Storage_setting.storage_seed door_seed=Storage_setting.door_seed pos_seed=Storage_setting.pos_seed storage_pack=Storage_setting.storage_pack door_pack=Storage_setting.door_pack pos_pack=Storage_setting.pos_pack webhook_url_pack=Webhook_setting.webhook_url_pack message_id_pack=Webhook_setting.message_id_pack webhook_url_seed=Webhook_setting.webhook_url_seed message_id_seed=Webhook_setting.message_id_seed webhook_url_status=Webhook_setting.webhook_url_status delay_harvest=Delay_setting.delay_harvest delay_plant=Delay_setting.delay_plant delay_break=Delay_setting.delay_break delay_put=Delay_setting.delay_put tile_for_pnb=PNB_setting.tile_for_pnb custom_pos_for_pnb=PNB_setting.custom_pos_for_pnb custom_pos_x=PNB_setting.custom_pos_x custom_pos_y=PNB_setting.custom_pos_y pack=Pack_setting.pack pack_list=Pack_setting.pack_list pack_name=Pack_setting.pack_name minimum_gem=Pack_setting.minimum_gem pack_price=Pack_setting.pack_price pack_limit=Pack_setting.pack_limit goods=Other_setting.trash_whitelist looping=Other_setting.looping custom_show_world=Other_setting.custom_show_world custom_show_world_amount=Other_setting.custom_show_world_amount start=Bots[string.upper((bot:getLocal()).name)].start_from stop=#Bots[string.upper((bot:getLocal()).name)].world_list door_farm=Bots[string.upper((bot:getLocal()).name)].door_farm webhook_url=Bots[string.upper((bot:getLocal()).name)].webhook_url message_id=Bots[string.upper((bot:getLocal()).name)].message_id world_list=Bots[string.upper((bot:getLocal()).name)].world_list total_list=#Bots[string.upper((bot:getLocal()).name)].world_list for O=start,stop,1 do table.insert(worlds,world_list[O])end if looping then for O=0,start-1,1 do table.insert(worlds,world_list[O])end end for O,c in pairs(pack_list)do table.insert(goods,c)end for O=math.floor(tile_for_pnb/2),1,-1 do O=O*-1 table.insert(tile_break,O)end for O=0,math.ceil(tile_for_pnb/2)-1,1 do table.insert(tile_break,O)end if custom_show_world_amount-1>=#world_list then custom_show_world=false end function logs(O)log(string.format("\091\037\115\093 \037\115 \n",(bot:getLocal()).name,O))end function getposx()return math.floor((bot:getLocal()).pos.x/32)end function getposy()return math.floor((bot:getLocal()).pos.y/32)end function bl(O)fossil[O]=0 for c,X in ipairs(bot:getTiles())do if X.fg==3918 then fossil[O]=fossil[O]+1 end end end function tile_punch(O,c)for X,b in pairs(tile_break)do if(bot:getTile(O-1,c+b)).fg~=0 or(bot:getTile(O-1,c+b)).bg~=0 then return true end end return false end function tile_place(O,c)for X,b in pairs(tile_break)do if(bot:getTile(O-1,c+b)).fg==0 and(bot:getTile(O-1,c+b)).bg==0 then return true end end return false end function warp(O,c)while string.upper((bot:getWorld()).name)~=string.upper(O)do bot:sendPacket("\097\099\116\105\111\110\124\106\111\105\110\095\114\101\113\117\101\115\116\n\110\097\109\101\124"..(string.upper(O).."\n\105\110\118\105\116\101\100\087\111\114\108\100\124\048"),3)sleep(6000)end if c==""or c==nil then return end while(bot:getTile(math.floor((bot:getLocal()).pos.x/32),math.floor((bot:getLocal()).pos.y/32))).fg==6 do bot:sendPacket("\097\099\116\105\111\110\124\106\111\105\110\095\114\101\113\117\101\115\116\n\110\097\109\101\124"..(string.upper(O)..("\124"..(string.upper(c).."\n\105\110\118\105\116\101\100\087\111\114\108\100\124\048"))),3)sleep(6000)end end function waktu_world()str_waktu=""if custom_show_world then for O=custom_show_world_amount,1,-1 do new_list=list_now-O if new_list<=0 then new_list=new_list+total_list end str_waktu=str_waktu..("\092\110"..(string.upper(world_list[new_list])..(" \040 "..((waktu[world_list[new_list]]or"\063")..(" \124 "..((tree[world_list[new_list]]or"\063").." \041"))))))end else for O,c in pairs(world_list)do str_waktu=str_waktu..("\092\110"..(string.upper(c)..(" \040 "..((waktu[c]or"\063")..(" \124 "..((tree[c]or"\063").." \041"))))))end end end function bstatus()local O=bot:getBotStatus()if O==Success then return"\083\117\099\099\101\115\115"elseif O==Disconnected then return"\068\105\115\099\111\110\110\101\099\116\101\100"elseif O==OnSendToServer then return"\079\110 \083\101\110\100 \084\111 \083\101\114\118\101\114"elseif O==Banned then return"\066\097\110\110\101\100"elseif O==Suspended then return"\083\117\115\112\101\110\100\101\100"elseif O==Update_Required then return"\085\112\100\097\116\101 \082\101\113\117\105\114\101\100"elseif O==Maintenance then return"\077\097\105\110\116\101\110\097\110\099\101"elseif O==SERVER_OVERLOADED then return"\083\101\118\101\114 \079\118\101\114\108\111\097\100\101\100"else return 0 end end function estatus()local O=bot:getEnetStatus()if O==Connected then return"\067\111\110\110\101\099\116\101\100"elseif O==Disconnected then return"\068\105\115\099\111\110\110\101\099\116\101\100"elseif O==ShadowBan then return"\083\104\097\100\111\119 \066\097\110"else return 0 end end function format_number(O)local c=tostring(O)local X=string.len(c)for O=X-3,1,-3 do c=string.sub(c,1,O)..("\046"..string.sub(c,O+1))end return c end function bot_info(O)te=os.time()-t fossill=fossil[bot:getCurrentWorld()]or 0 local c="        \123\n            \"\099\111\110\116\101\110\116\"\058 \110\117\108\108\044\n            \"\101\109\098\101\100\115\"\058 \091\n              \123\n                \"\116\105\116\108\101\"\058 \"\042\042\082\111\116\097\116\105\111\110 \066\121 \077\097\114\114\042\042\"\044\n                \"\099\111\108\111\114\"\058 \048\044\n                \"\102\105\101\108\100\115\"\058 \091\n                  \123\n                    \"\110\097\109\101\"\058 \"\095\095\066\079\084 \073\078\070\079\082\077\065\084\073\079\078\095\095\"\044\n                    \"\118\097\108\117\101\"\058 \"\060\058\105\099\111\110\095\105\110\102\111\058\049\049\051\056\052\056\049\052\054\049\054\055\057\051\055\052\052\051\055\062 \042\042\084\097\115\107\042\042 \058 "..(O..("\092\110\060\058\105\110\102\111\114\109\097\116\105\111\110\058\049\049\051\056\052\056\049\055\054\056\048\052\051\057\050\053\053\050\052\062 \042\042\066\111\116 \078\097\109\101\042\042 \058 "..((bot:getLocal()).name..(" \042\042\040"..((bot:getLocal()).level..("\041\042\042\092\110\060\058\111\102\102\108\105\110\101\095\100\101\115\107\116\111\112\058\049\049\051\056\052\056\051\054\054\055\056\053\051\057\049\052\049\054\050\062 \042\042\066\111\116 \083\116\097\116\117\115\042\042 \058 "..(bstatus()..("\092\110\060\058\111\102\102\108\105\110\101\095\119\101\098\058\049\049\051\056\052\056\051\057\052\053\048\050\054\048\057\055\050\052\050\062 \042\042\069\078\101\116 \083\116\097\116\117\115\042\042 \058 "..(estatus()..("\092\110\060\058\115\105\108\118\101\114\058\049\049\051\056\052\056\052\057\050\051\056\055\053\057\057\057\055\055\052\062 \042\042\071\101\109\115\042\042 \058 "..(format_number((bot:getLocal()).gems)..("\092\110\060\058\103\108\111\098\101\115\058\049\049\051\056\053\048\051\055\057\056\048\057\055\049\056\054\056\056\055\062 \042\042\087\111\114\108\100 \042\042 \058 "..(bot:getCurrentWorld()..(" \042\042\040"..(fossill..("\041\042\042\092\110 \060\058\116\111\100\111\095\108\105\115\116\058\049\049\051\056\052\057\056\049\049\050\048\052\050\056\051\050\048\049\051\062 \042\042\087\111\114\108\100 \079\114\100\101\114\042\042 \058 "..(start..(" \047 "..(stop..("\"\044\n                    \"\105\110\108\105\110\101\"\058 \116\114\117\101\n                  \125\044\n                  \123\n                    \"\110\097\109\101\"\058 \"\095\095\080\065\067\075 \073\078\070\079\082\077\065\084\073\079\078\095\095\"\044\n                    \"\118\097\108\117\101\"\058 \"\060\058\083\104\111\112\112\105\110\103\099\097\114\116\058\049\049\051\056\052\057\053\057\048\049\050\049\054\056\048\056\057\057\048\062 \042\042\080\097\099\107 \078\097\109\101\042\042 \058 "..(pack..("\092\110\060\058\100\111\108\108\097\114\115\