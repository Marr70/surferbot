x()]=B end end return X end function punch(B,X)local W=bot:getLocal()return bot:hitTile(math.floor(W.pos.x/32)+B,math.floor(W.pos.y/32)+X)end function break_toxic_waste()waste_tiles={}for B,X in ipairs(bot:getTiles())do if X.fg==778 or X.bg==778 then table.insert(waste_tiles,{x=X.x;y=X.y})end end if#waste_tiles>0 then logs("\067\108\101\097\114\105\110\103 \084\111\120\105\099 \087\097\115\116\101")while#waste_tiles>0 do for B,X in pairs(waste_tiles)do if bot:findPath(X.x,X.y+1)then sleep(500)punch(0,-1)sleep(1000)table.remove(waste_tiles,B)elseif bot:findPath(X.x,X.y-1)then sleep(500)punch(0,1)sleep(1000)table.remove(waste_tiles,B)elseif bot:findPath(X.x+1,X.y)then sleep(500)punch(-1,0)sleep(1000)table.remove(waste_tiles,B)elseif bot:findPath(X.x-1,X.y)then sleep(500)punch(1,0)sleep(1000)table.remove(waste_tiles,B)end end end logs("\068\111\110\101 \067\108\101\097\114\105\110\103 \084\111\120\105\099 \087\097\115\116\101")end end function bstatus()return bot:getBotStatus()end function estatus()return bot:getEnetStatus()end function cek_multibot(B,X)for B,W in pairs(B)do if string.upper(W.name)==string.upper(X)then return true end end return false end function main()midX,midY=50,27 goods={}check_geiger()if stop_script then return end if charging then return end tele(geiger_world,geiger_id)if multibot then for B,X in pairs(duplicate_line)do if not(getBot(X)):isEquipped(2204)then while(getBot(X)):getItemCount(2204)==0 and(getBot(X)):getItemCount(2286)==0 do sleep(2000)end end end for B,X in pairs(duplicate_line)do if string.upper(X)==string.upper(((getBot()):getLocal()).name)then break end if string.upper(X)~=string.upper(((getBot()):getLocal()).name)then while(getBot(X)):getItemCount(2286)==0 do sleep(2000)if(getBot(X)):getEnetStatus()~=Connected and(getBot(X)):getBotStatus()~=Success then break end end end end end sleep(2000)break_toxic_waste()reserve_items()blup=false bldown=false blright=false blleft=false bltopright=false blbottomright=false blbottomleft=false b