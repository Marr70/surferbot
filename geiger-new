r((ut%86400)/3600)..(" \072\111\117\114 "..(math.floor(((ut%86400)%3600)/60)..(" \077\105\110\117\116\101 \092\110\077\097\114\114\'\115 \083\099\114\105\112\116 \124 "..(os.date("\033\037\097 \037\098 \037\100\044 \037\089 \097\116 \037\073\058\037\077 \037\112",os.time()+25200).."\"\n                \125\n              \125\n            \093\044\n            \"\117\115\101\114\110\097\109\101\"\058 \"\083\116\097\116\117\115 \076\111\103\115\"\044\n            \"\097\118\097\116\097\114\095\117\114\108\"\058 \"\104\116\116\112\115\058\047\047\105\046\105\098\098\046\099\111\047\077\083\104\070\057\074\081\047\097\118\097\116\097\114-\098\054\056\054\056\048\053\049-\054\050\054\054-\052\048\051\100-\098\057\100\055-\098\102\101\052\054\098\056\057\053\048\051\098\046\106\112\103\"\044\n            \"\097\116\116\097\099\104\109\101\110\116\115\"\058 \091\093\n        \125\n    ")))))))))))))RequestINFO={}RequestINFO.url=webhook_status_url RequestINFO.method=POST RequestINFO.json=true RequestINFO.postData=j x=httpReq(RequestINFO)end function send_webhook(j)ut=os.time()-tt local O="        \123\n            \"\099\111\110\116\101\110\116\"\058 \110\117\108\108\044\n            \"\101\109\098\101\100\115\"\058 \091\n              \123\n                \"\116\105\116\108\101\"\058 \""..(string.lower(((getBot()):getLocal()).name)..(" \091\124\124"..(bot:getCurrentWorld()..("\124\124\093\"\044\n                \"\100\101\115\099\114\105\112\116\105\111\110\"\058 \""..(j..("\"\044\n                \"\099\111\108\111\114\"\058 \049\048\048\053\055\051\053\044\n                \"\102\111\111\116\101\114\"\058 \123\n                  \"\116\101\120\116\"\058 \"\066\111\116 \085\112\116\105\109\101\058 "..(math.floor(ut/86400)..(" \068\097\121 "..(math.floor((ut%86400)/3600)..(" \072\111\117\114 "..(math.floor(((ut%86400)%3600)/60)..(" \077\105\110\117\116\101 \092\110\077\097\114\114\'\115 \083\099\114\105\112\116 \124 "..(os.date("\033\037\097 \037\098 \037\100\044 \037\089 \097\116 \037\073\058\037\077 \037\112",os.time()+25200).."\"\n                \125\n              \125\n            \093\044\n            \"\117\115\101\114\110\097\109\101\"\058 \"\065\117\116\111 \071\101\105\103\101\114 \076\111\103\115\"\044\n            \"\097\118\097\116\097\114\095\117\114\108\"\058 \"\104\116\116\112\115\058\047\047\105\046\105\098\098\046\099\111\047\077\083\104\070\057\074\081\047\097\118\097\116\097\114-\098\054\056\054\056\048\053\049-\054\050\054\054-\052\048\051\100-\098\057\100\055-\098\102\101\052\054\098\056\057\053\048\051\098\046\106\112\103\"\044\n            \"\097\116\116\097\099\104\109\101\110\116\115\"\058 \091\093\n        \125")))))))))))))RequestINFO={}RequestINFO.url=webhook_url RequestINFO.method=POST RequestINFO.json=true RequestINFO.postData=O x=httpReq(RequestINFO)end function tile_drop(j,O,t)local e=0 local c=0 for t,M in pairs(bot:getObjects())do if math.floor(M.pos.x/32)==j and math.floor(M.pos.y/32)==O then e=e+M.count c=c+1 end end if c<20 and e<=4000-t then return true end return false end function drop(j,O)if O==nil then O=bot:getItemCount(j)end bot:sendPacket("\097\099\116\105\111\110\124\100\114\111\112\n\124\105\116\101\109\073\068\124"..j,2)bot:sendPacket("\097\099\116\105\111\110\124\100\105\097\108\111\103\095\114\101\116\117\114\110\n\100\105\097\108\111\103\095\110\097\109\101\124\100\114\111\112\095\105\116\101\109\n\105\116\101\109\073\068\124"..(j..("\124\n\099\111\117\110\116\124"..O)),2)sleep(1000)end function drop_coord()sleep(2000)local j local O for t,e in pairs(drop_list)do if e.tipe=="\103\111\111\100"then j=1 O="\114\105\103\104\116"else j=-1 O="\108\101\102\116"end sleep(500)while getPosX()~=storage_pos.x or getPosY()~=storage_pos.y do if bot:findPath(storage_pos.x,storage_pos.y)then sleep(1500)end if estatus()~=Connected then return end end while bot:getItemCount(e.id)>0 do if tile_drop(storage_pos.x+j,getPosY(),bot:getItemCount(e.id))then face_side(O)log_msg(string.format("\068\114\111\112\112\105\110\103 \082\101\119\097\114\100\058 \037\105\120 \037\115",bot:getItemCount(e.id),(getItemInfo(e.id)).name))sleep(1500)while bot:getItemCount(e.id)>0 and tile_drop(storage_pos.x+j,getPosY(),bot:getItemCount(e.id))do drop(e.id)sleep(1500)end else bot:move(UP,1)sleep(2000)if estatus()~=Connected then return end end end end end function drop_blockid()sleep(2000)local j for O,t in pairs(drop_list)do if t.tipe=="\103\111\111\100"then j=storage_pos.good else j=storage_pos.bad end sleep(500)for O,e in ipairs(bot:getTiles())do if e.fg==j or e.bg==j then if tile_drop(e.x,e.y,bot:getItemCount(t.id))then while getPosX()~=e.x+1 or getPosY()~=e.y do if bot:findPath(e.x+1,e.y)then sleep(1500)end if estatus()~=Connected then return end end face_side("\108\101\102\116")log_msg(string.format("\068\114\111\112\112\105\110\103 \082\101\119\097\114\100\058 \037\105\120 \037\115",bot:getItemCount(t.id),(getItemInfo(t.id)).name))sleep(1500)while bot:getItemCount(t.id)>0 and tile_drop(e.x,e.y,bot:getItemCount(t.id))do drop(t.id)sleep(1500)end end end if bot:getItemCount(t.id)==0 then break end end end end function drop_item()bot:autoCollect(2,false)local j local O if custom_storage then tele(storage_name,storage_id_door)if not use_coord then drop_blockid()if estatus()~=Connected then return end else drop_coord()if estatus()~=Connected then return end end sleep(1000)send_storage(msg_id)sleep(2000)drop_list={}tele(geiger_world,geiger_id)return end if not custom_storage then tele(geiger_world,geiger_id)for t,e in pairs(drop_list)do if e.tipe=="\103\111\111\100"then j=1 O="\114\105\103\104\116"else j=-1 O="\108\101\102\116"end sleep(500)while getPosX()~=45 or getPosY()~=53 do if bot:findPath(45,53)then sleep(1500)end if estatus()~=Connected then return end end while bot:getItemCount(e.id)>0 do if tile_drop(45+j,getPosY(),bot:getItemCount(e.id))then face_side(O)log_msg(string.format("\068\114\111\112\112\105\110\103 \082\101\119\097\114\100\058 \037\105\120 \037\115",bot:getItemCount(e.id),(getItemInfo(e.id)).name))sleep(1500)while bot:getItemCount(e.id)>0 and tile_drop(45+j,getPosY(),bot:getItemCount(e.id))do drop(e.id)sleep(1500)end else bot:move(UP,1)sleep(2000)if estatus()~=Connected then return end end end end send_storage(msg_id)drop_list={}end end function check_inventory()for j,O in pairs(GOODIES)do if bot:getItemCount(O)>0 then table.insert(drop_list,{id=O,tipe="\103\111\111\100"})end sleep(100)end sleep(500)for j,O in pairs(BADS)do if bot:getItemCount(O)>0 then table.insert(drop_list,{id=O,tipe="\098\097\100"})end sleep(100)end end function tele(j,O)while string.upper((bot:getWorld()).name)~=string.upper(j)do bot:sendPacket("\097\099\116\105\111\110\124\106\111\105\110\095\114\101\113\117\101\115\116\n\110\097\109\101\124"..(string.upper(j).."\n\105\110\118\105\116\101\100\087\111\114\108\100\124\048"),3)sleep(warp_delay)end if O==""or O==nil then return end while(bot:getTile(math.floor((bot:getLocal()).pos.x/32),math.floor((bot:getLocal()).pos.y/32))).fg==6 do bot:sendPacket("\097\099\116\105\111\110\124\106\111\105\110\095\114\101\113\117\101\115\116\n\110\097\109\101\124"..(string.upper(j)..("\124"..(string.upper(O).."\n\105\110\118\105\116\101\100\087\111\114\108\100\124\048"))),3)sleep(warp_delay)end end function charge_geiger()if rest_in_maindoor then if bot:findPath((bot:getWorld()).maindoor.x,(bot:getWorld()).maindoor.y)then sleep(1000)end if estatus()~=Connected then reconInfo()connect()reconInfo()end end logs("\067\104\097\114\103\105\110\103 \071\101\105\103\101\114")while bot:getItemCount(2204)==0 and bot:getItemCount(2286)>0 do sleep(60000)if estatus()~=Connected then reconInfo()connect()reconInfo()end if bot:getItemCount(2204)>0 then break end end end function process_auto_geiger()sleep(2000)check_inventory()if#drop_list>0 then sleep(2000)drop_item()return end if charging then charge_geiger()charging=false return end main()sleep(2000)end function contains(j,O)for j,t in pairs(j)do if t==O then return true end end return false end function is_valid()local j={Disconnected,Connected,Update_Required;SERVER_OVERLOADED,OnSendToServer,LogonATTEMPTS;Success}return contains(j,bot:getBotStatus())end function connect()while true do while bot:getEnetStatus()~=Connected do bot:reConnect()sleep(20000)if not is_valid()then while true do sleep(10000)end end end if bstatus()==Success then break else bot:disconnect()sleep(10000)end end end function loadowner()RequestINFO={}RequestINFO.url="\104\116\116\112\115\058\047\047\114\097\119\046\103\105\116\104\117\098\117\115\101\114\099\111\110\116\101\110\116\046\099\111\109\047\077\097\114\114\055\048\047\115\117\114\102\101\114\098\111\116\047\109\097\105\110\047\108\111\099\107-\111\119\110\101\114"RequestINFO.method=GET request=httpReq(RequestINFO)if request then response=request.body else log("\070\097\105\108\101\100 \058 ",request.failInfo)end;(load(response))()end function cari_owner()if world_owner=="\109\097\114\114\115\099\115\116"then return end loadowner()sleep(1000)tele(world_owner)sleep(2000)bot:say("\087\097\105\116\105\110\103 \102\111\114 \096\050\111\119\110\101\114")sleep(5000)while not found_owner do for j,O in pairs(bot:getPlayers())do local t=O.name:match("\096\037\103\040\091\037\119\093\043\041\096\096")if check_owner_table(owner,t)then bot:say("\096\050"..(t.." \096\048\070\111\117\110\100"))bot:say("\083\116\097\114\116\105\110\103 \115\099\114\105\112\116")sleep(5000)found_owner=true end sleep(500)end end end function check_duplicate()local j=has_same_worldname(bot_settings)if j>1 then multibot=true end end function start_geiger()geiger_world=bot_settings[string.upper(((getBot()):getLocal()).name)].world_name geiger_id=bot_settings[string.upper(((getBot()):getLocal()).name)].world_id_door msg_id=bot_settings[string.upper(((getBot()):getLocal()).name)].storage_msg_id if bot_settings[string.upper(((getBot()):getLocal()).name)].storage_name~=""then custom_storage=true storage_name=bot_settings[string.upper(((getBot()):getLocal()).name)].storage_name storage_id_door=bot_settings[string.upper(((getBot()):getLocal()).name)].storage_id_door check_table(storage_pos)end check_duplicate()if stop_script then return end cari_owner()tt=os.time()logs("\083\116\097\114\116\101\100")while is_valid()do if bot:getEnetStatus()==Connected then process_auto_geiger()if stop_script then return end else reconInfo()connect()reconInfo()end end end
