5")))))))))))))))))))))))))))))))))))))local U={url=webhookMain..("\047\109\101\115\115\097\103\101\115\047"..messageId);method=PATCH,json=true,postData=l}httpReq(U)end function reconInfo()local V="        \123\n        \"\099\111\110\116\101\110\116\"\058 \"\124\124"..((bot:getLocal()).name..("\124\124 \115\116\097\116\117\115 \105\115 "..(statusBot().." \064\101\118\101\114\121\111\110\101\"\044\n        \"\101\109\098\101\100\115\"\058 \110\117\108\108\044\n        \"\117\115\101\114\110\097\109\101\"\058 \"\079\109\110\105\115\099\105\117\115 \083\099\114\105\112\116\"\044\n        \"\097\116\116\097\099\104\109\101\110\116\115\"\058 \091\093\n    \125\n    ")))local t={url=webhookStatus,method=POST,json=true,postData=V}httpReq(t)end function statusBot()bot=getBot()if bot:getEnetStatus()==Connected and bot:getBotStatus()==Success then Status="\111\110\108\105\110\101"elseif bot:getBotStatus()==Suspended then Status="\115\117\115\112\101\110\100\101\100"elseif bot:getBotStatus()==Banned then Status="\098\097\110\110\101\100"elseif bot:getBotStatus()==Wrong_Password then Status="\119\114\111\110\103"elseif bot:getBotStatus()==Update_Required then Status="\117\112\100\097\116\101 \118\101\114\115\105\111\110"elseif bot:getBotStatus()==Maintenance then Status="\109\097\105\110\116\101\110\097\110\099\101"elseif bot:getBotStatus()==AAP then Status="\065\065\080"elseif bot:getBotStatus()==Unable_to_create_new_account then Status="\103\117\101\115\116\069\114\114\111\114"else Status="\111\102\102\108\105\110\101"end return Status end function reconnect(V,t,l,U)if statusBot()~="\111\110\108\105\110\101"then botInfo("\082\101\099\111\110\110\101\099\116\105\110\103")sleep(100)reconInfo()sleep(100)konek=0 while true do bot:reConnect()sleep(15000)if statusBot()=="\115\117\115\112\101\110\100\101\100"or statusBot()=="\098\097\110\110\101\100"then botInfo(statusBot())sleep(100)reconInfo()sleep(100)while true do sleep(10000)end end while statusBot()=="\111\110\108\105\110\101"and(bot:getWorld()).name:upper()~=V:upper()do bot:sendPacket("\097\099\116\105\111\110\124\106\111\105\110\095\114\101\113\117\101\115\116\n\110\097\109\101\124"..(V:upper().."\n\105\110\118\105\116\101\100\087\111\114\108\100\124\048"),3)sleep(delayJoinWorld)end if statusBot()=="\111\110\108\105\110\101"and(bot:getWorld()).name:upper()==V:upper()then if t then while(bot:getTile(math.floor((bot:getLocal()).pos.x/32),math.floor((bot:getLocal()).pos.y/32))).fg==6 do bot:sendPacket("\097\099\116\105\111\110\124\106\111\105\110\095\114\101\113\117\101\115\116\n\110\097\109\101\124"..(V:upper()..("\124"..(t:upper().."\n\105\110\118\105\116\101\100\087\111\114\108\100\124\048"))),3)sleep(delayJoinWorld)end end if l and(U and(statusBot()=="\111\110\108\105\110\101"and(bot:getWorld()).name:upper()==V:upper()))then while math.floor((bot:getLocal()).pos.x/32)~=l or math.floor((bot:getLocal()).pos.y/32)~=U do if not bot:isInside(l,U,0)and#bot:getPath(l,U)>0 then bot:findPath(l,U)sleep(200)end end end if statusBot()=="\111\110\108\105\110\101"and(bot:getWorld()).name:upper()==V:upper()then if l and U then if statusBot()=="\111\110\108\105\110\101"and(math.floor((bot:getLocal()).pos.x/32)==l and math.floor((bot:getLocal()).pos.y/32)==U)then break end elseif statusBot()=="\111\110\108\105\110\101"then break end end end if konek<20 then konek=konek+1 else botInfo("\077\097\120 \082\101\099\111\110\110\101\099\116 \065\116\116\101\109\112\116 \082\101\097\099\104\101\100\033 \087\097\105\116\105\110\103 \102\111\114 "..(math.floor(16.666666666667)/100 .." \077\105\110\117\116\101\115"))sleep(600000)konek=0 botInfo("\082\101\099\111\110\110\101\099\116\105\110\103")sleep(100)end end botInfo("\083\117\099\099\101\115\102\117\108\108\121 \082\101\099\111\110\110\101\099\116\101\100")sleep(100)reconInfo()sleep(100)botInfo("\070\097\114\109\105\110\103")sleep(100)end end function findItem(V)return bot:getItemCount(V)end function round(V)return V%1>.5 and math.ceil(V)or math.floor(V)end function tileDrop1(V,t,l)local U=0 local B=0 for l,u in pairs(bot:getObjects())do if round(u.pos.x/32)==V and math.floor(u.pos.y/32)==t then U=U+u.count B=B+1 end end if B<20 and U<=4000-l then return true end return false end function tileDrop2(V,t,l)local U=0 local B=0 for l,u in pairs(bot:getObjects())do if round(u.pos.x/32)==V and math.floor(u.pos.y/32)==t then U=U+u.count B=B+1 end end if U<=4000-l then return true end return false end function storePack()for V,t in pairs(packList)do for V,l in ipairs(bot:getTiles({skip=true}))do if l.fg==patokanPack or l.bg==patokanPack then if tileDrop1(l.x,l.y,findItem(t))then while math.floor((bot:getLocal()).pos.x/32)~=l.x+1 or math.floor((bot:getLocal()).pos.y/32)~=l.y do bot:findPath(l.x+1,l.y)sleep(1000)reconnect(packStorage,doorPack,l.x+1,l.y)end while findItem(t)>0 and tileDrop1(l.x,l.y,findItem(t))do bot:sendPacket("\097\099\116\105\111\110\124\100\114\111\112\n\124\105\116\101\109\073\068\124"..t,2)sleep(1250)bot:sendPacket("\097\099\116\105\111\110\124\100\105\097\108\111\103\095\114\101\116\117\114\110\n\100\105\097\108\111\103\095\110\097\109\101\124\100\114\111\112\095\105\116\101\109\n\105\116\101\109\073\068\124"..(t..("\124\n\099\111\117\110\116\124"..findItem(t))),2)sleep(1250)reconnect(packStorage,doorPack,l.x-1,l.y)end end end if findItem(t)==0 then break end end end end function itemInfo()if seedId==4585 then emote="\060\058\112\101\112\112\101\114\115\058\049\050\053\054\049\051\054\057\049\051\049\049\049\051\053\053\052\053\051\062"elseif seedId==5667 then emote="\060\058\108\103\114\105\100\115\058\049\050\053\054\049\054\054\052\055\052\056\055\054\052\053\050\057\048\052\062"elseif seedId==341 then emote="\060\058\099\104\097\110\100\115\058\049\050\053\054\049\054\054\055\051\049\048\054\052\050\055\057\048\052\049\062"elseif seedId==8641 then emote="\060\058\102\101\110\099\101\115\058\049\050\053\054\049\054\055\049\050\056\051\049\053\051\051\052\054\053\056\062"elseif seedId==3005 then emote="\060\058\102\116\097\110\107\115\058\049\050\053\054\049\054\055\053\048\054\056\056\048\055\053\055\055\057\048\062"else emote="\060\058\105\100\107\058\049\050\053\054\049\054\055\055\053\051\052\051\056\052\054\050\048\055\054\062"end return emote end function infoPack()local V={}for t,l in pairs(bot:getObjects())do if l.id==seedId then if V[l.id]then V[l.id].count=V[l.id].count+l.count else V[l.id]={id=l.id;count=l.count}end end end countSeed=V[seedId].count end function storeSeed(V)botInfo("\083\116\111\114\105\110\103 \083\101\101\100")sleep(100)bot:autoCollect(3,false)sleep(100)warp(seedStorage,doorSeed)sleep(100)for V,t in ipairs(bot:getTiles({skip=true}))do if t.fg==patokanSeed or t.bg==patokanSeed then if tileDrop2(t.x,t.y,100)then while math.floor((bot:getLocal()).pos.x/32)~=t.x+1 or math.floor((bot:getLocal()).pos.y/32)~=t.y do bot:findPath(t.x+1,t.y)sleep(1000)reconnect(seedStorage,doorSeed,t.x+1,t.y)end while findItem(seedId)>=100 and tileDrop2(t.x,t.y,100)do bot:sendPacket("\097\099\116\105\111\110\124\100\114\111\112\n\124\105\116\101\109\073\068\124"..seedId,2)sleep(1250)bot:sendPacket("\097\099\116\105\111\110\124\100\105\097\108\111\103\095\114\101\116\117\114\110\n\100\105\097\108\111\103\095\110\097\109\101\124\100\114\111\112\095\105\116\101\109\n\105\116\101\109\073\068\124"..(seedId.."\124\n\099\111\117\110\116\124\049\048\048"),2)sleep(1250)reconnect(seedStorage,doorSeed,t.x+1,t.y)end end if findItem(seedId)<100 then break end end end infoPack()sleep(100)warp(V,doorFarm)sleep(100)bot:autoCollect(3,true)sleep(100)botInfo("\070\097\114\109\105\110\103")sleep(100)end function buy()botInfo("\066\117\121\105\110\103 \097\110\100 \083\116\111\114\105\110\103 \080\097\099\107")sleep(100)bot:autoCollect(3,false)sleep(100)warp(packStorage,doorPack)sleep(100)while(bot:getLocal()).gems>=packPrice do for V=1,20,1 do bot:sendPacket("\097\099\116\105\111\110\124\098\117\121\n\105\116\101\109\124"..packName,2)sleep(2000)if findItem(packList[1])==0 then bot:sendPacket("\097\099\116\105\111\110\124\098\117\121\n\105\116\101\109\124\117\112\103\114\097\100\101\095\098\097\099\107\112\097\099\107",2)sleep(1500)else profit=profit+1 end if(bot:getLocal()).gems<packPrice then break end end storePack()sleep(100)reconnect(packStorage,doorPack)end botInfo("\070\097\114\109\105\110\103")sleep(100)end function includesNumber(V,t)for V,l in pairs(V)do if l==t then return true end end return false end function clear()for V,t in pairs(bot:getInventory())do if not includesNumber(goods,t.id)then bot:sendPacket("\097\099\116\105\111\110\124\116\114\097\115\104\n\124\105\116\101\109\073\068\124"..t.id,2)sleep(500)bot:sendPacket("\097\099\116\105\111\110\124\100\105\097\108\111\103\095\114\101\116\117\114\110\n\100\105\097\108\111\103\095\110\097\109\101\124\116\114\097\115\104\095\105\116\101\109\n\105\116\101\109\073\068\124"..(t.id..("\124\n\099\111\117\110\116\124"..t.count)),2)sleep(1250)end end end function collect(V,t)for l,U in pairs(bot:getObjects())do if t~=""and t~=nil then if U.id==t then local t=round(U.pos.x/32)local l=math.floor(U.pos.y/32)if bot:isInside(t,l,V)then bot:collectObject(U.uid)end end else local t=round(U.pos.x/32)local l=math.floor(U.pos.y/32)if bot:isInside(t,l,V)then bot:collectObject(U.uid)end end end end function take(V)botInfo("\084\097\107\105\110\103 \083\101\101\100")sleep(100)while findItem(seedId)==0 do bot:autoCollect(3,false)sleep(100)warp(seedStorage,doorSeed)sleep(100)for V,t in pairs(bot:getObjects())do if t.id==seedId then bot:findPath(round(t.pos.x/32),math.floor(t.pos.y/32))sleep(1000)collect(2,seedId)sleep(1000)end if findItem(seedId)>0 then break end end warp(V,doorFarm)sleep(100)bot:autoCollect(3,true)sleep(100)end end function place(V,t,l)return bot:placeTile(math.floor((bot:getLocal()).pos.x/32)+t,math.floor((bot:getLocal()).pos.y/32)+l,V)end function punch(V,t)return bot:hitTile(math.floor((bot:getLocal()).pos.x/32)+V,math.floor((bot:getLocal()).pos.y/32)+t)end function checkSeed(V)if findItem(seedId)==0 then take(V)sleep(100)botInfo("\080\108\097\110\116\105\110\103 \083\101\101\100\115")sleep(100)end end function plant(V)botInfo("\080\108\097\110\116\105\110\103 \083\101\101\100\115")sleep(100)for t,l in ipairs(bot:getTiles())do if findItem(seedId)==0 then break end if(bot:getTile(l.x,l.y)).fg==0 and(l.y~=0 and((bot:getTile(l.x,l.y+1)).fg~=0 and(not bot:isSeed(l.x,l.y+1)and(bot:hasAccessToTile(l.x,l.y)and(l.x~=0 and l.x~=99)))))then bot:findPath(l.x,l.y)sleep(50)while(bot:getTile(l.x,l.y)).fg==0 and(bot:hasAccessToTile(l.x,l.y)and(bot:getTile(l.x,l.y+1)).fg~=0)do place(seedId,0,0)sleep(delayPlant)reconnect(V,doorFarm,l.x,l.y)end elseif l.y==53 then if l.x==0 and(l.y==53 and(l.fg==0 and bot:hasAccessToTile(l.x,l.y)))then bot:findPath(l.x,l.y)sleep(50)while(bot:getTile(l.x,l.y)).fg==0 and bot:hasAccessToTile(l.x,l.y)do place(seedId,0,0)sleep(delayPlant)reconnect(V,doorFarm,l.x,l.y)end elseif(bot:getTile(l.x,l.y-1)).fg~=0 and((bot:getTile(l.x,l.y)).fg==0 and(bot:hasAccessToTile(l.x,l.y)and(l.x~=0 and l.x~=99)))then bot:findPath(l.x,l.y)sleep(50)while(bot:getTile(l.x,l.y)).fg==0 and(bot:hasAccessToTile(l.x,l.y)and(bot:getTile(l.x,l.y-1)).fg~=0)do place(seedId,0,0)sleep(delayPlant)reconnect(V,doorFarm,l.x,l.y+1)end elseif l.x==99 and(l.y==53 and(l.fg==0 and bot:hasAccessToTile(l.x,l.y)))then bot:findPath(l.x,l.y)sleep(50)while(bot:getTile(l.x,l.y)).fg==0 and bot:hasAccessToTile(l.x,l.y)do place(seedId,0,0)sleep(delayPlant)reconnect(V,doorFarm,l.x,l.y)end end end end if findItem(seedId)>=100 then storeSeed(V)sleep(100)end end function plant2(V)for t,l in ipairs(bot:getTiles())do if(bot:getTile(l.x,l.y)).fg==0 and(l.y~=0 and((bot:getTile(l.x,l.y+1)).fg~=0 and(not bot:isSeed(l.x,l.y+1)and(bot:hasAccessToTile(l.x,l.y)and(l.x~=0 and l.x~=99)))))then checkSeed(V)bot:findPath(l.x,l.y)sleep(50)while(bot:getTile(l.x,l.y)).fg==0 and(bot:hasAccessToTile(l.x,l.y)and(bot:getTile(l.x,l.y+1)).fg~=0)do place(seedId,0,0)sleep(delayPlant)reconnect(V,doorFarm,l.x,l.y)end elseif l.y==53 then if l.x==0 and(l.y==53 and(l.fg==0 and bot:hasAccessToTile(l.x,l.y)))then checkSeed(V)bot:findPath(l.x,l.y)sleep(50)while(bot:getTile(l.x,l.y)).fg==0 and bot:hasAccessToTile(l.x,l.y)do place(seedId,0,0)sleep(delayPlant)reconnect(V,doorFarm,l.x,l.y)end elseif(bot:getTile(l.x,l.y-1)).fg~=0 and((bot:getTile(l.x,l.y)).fg==0 and(bot:hasAccessToTile(l.x,l.y)and(l.x~=0 and l.x~=99)))then checkSeed(V)bot:findPath(l.x,l.y)sleep(50)while(bot:getTile(l.x,l.y)).fg==0 and(bot:hasAccessToTile(l.x,l.y)and(bot:getTile(l.x,l.y-1)).fg~=0)do place(seedId,0,0)sleep(delayPlant)reconnect(V,doorFarm,l.x,l.y+1)end elseif l.x==99 and(l.y==53 and(l.fg==0 and bot:hasAccessToTile(l.x,l.y)))then checkSeed(V)bot:findPath(l.x,l.y)sleep(50)while(bot:getTile(l.x,l.y)).fg==0 and bot:hasAccessToTile(l.x,l.y)do place(seedId,0,0)sleep(delayPlant)reconnect(V,doorFarm,l.x,l.y)end end end end if findItem(seedId)>=100 then storeSeed(V)sleep(100)end end function plant3(V)for t,l in ipairs(bot:getTiles())do if findItem(seedId)==0 then countWorld(V)sleep(100)if scanBlock()>0 then pnb2(V)sleep(100)else take(V)sleep(100)botInfo("\080\108\097\110\116\105\110\103 \083\101\101\100\115")sleep(100)end end if(bot:getTile(l.x,l.y)).fg==0 and(l.y~=0 and((bot:getTile(l.x,l.y+1)).fg~=0 and(not bot:isSeed(l.x,l.y+1)and(bot:hasAccessToTile(l.x,l.y)and(l.x~=0 and l.x~=99)))))then bot:findPath(l.x,l.y)sleep(50)while(bot:getTile(l.x,l.y)).fg==0 and(bot:hasAccessToTile(l.x,l.y)and(bot:getTile(l.x,l.y+1)).fg~=0)do place(seedId,0,0)sleep(delayPlant)reconnect(V,doorFarm,l.x,l.y)end elseif l.y==53 then if l.x==0 and(l.y==53 and(l.fg==0 and bot:hasAccessToTile(l.x,l.y)))then bot:findPath(l.x,l.y)sleep(50)while(bot:getTile(l.x,l.y)).fg==0 and bot:hasAccessToTile(l.x,l.y)do place(seedId,0,0)sleep(delayPlant)reconnect(V,doorFarm,l.x,l.y)end elseif(bot:getTile(l.x,l.y-1)).fg~=0 and((bot:getTile(l.x,l.y)).fg==0 and(bot:hasAccessToTile(l.x,l.y)and(l.x~=0 and l.x~=99)))then bot:findPath(l.x,l.y)sleep(50)while(bot:getTile(l.x,l.y)).fg==0 and(bot:hasAccessToTile(l.x,l.y)and(bot:getTile(l.x,l.y-1)).fg~=0)do place(seedId,0,0)sleep(delayPlant)reconnect(V,doorFarm,l.x,l.y+1)end elseif l.x==99 and(l.y==53 and(l.fg==0 and bot:hasAccessToTile(l.x,l.y)))then bot:findPath(l.x,l.y)sleep(50)while(bot:getTile(l.x,l.y)).fg==0 and bot:hasAccessToTile(l.x,l.y)do place(seedId,0,0)sleep(delayPlant)reconnect(V,doorFarm,l.x,l.y)end end end end if findItem(seedId)>=100 then storeSeed(V)sleep(100)end end function pnb(V)if findItem(blockId)>=tileNumber then botInfo("\066\114\101\097\107\105\110\103 \066\108\111\099\107\115")if not customPNB then ex=1 ye=math.floor((bot:getLocal()).pos.y/32)if ye>40 then ye=ye-10 elseif ye<11 then ye=ye+10 end if(bot:getTile(ex,ye)).fg~=0 and(bot:getTile(ex,ye)).fg~=seedId then ye=ye-1 end else ex=customX ye=customY end while math.floor((bot:getLocal()).pos.x/32)~=ex or math.floor((bot:getLocal()).pos.y/32)~=ye do bot:findPath(ex,ye)sleep(1500)end if tileNumber>1 then while findItem(blockId)>=tileNumber and findItem(seedId)<190 do while tilePlace(ex,ye)do for t,l in pairs(tileBreak)do if(bot:getTile(ex-1,ye+l)).fg==0 and(bot:getTile(ex-1,ye+l)).bg==0 then place(blockId,-1,l)sleep(delayPlace)reconnect(V,doorFarm,ex,ye)end end end while tilePunch(ex,ye)do for t,l in pairs(tileBreak)do if(bot:getTile(ex-1,ye+l)).fg~=0 or(bot:getTile(ex-1,ye+l)).bg~=0 then punch(-1,l)sleep(delayPunch)reconnect(V,doorFarm,ex,ye)end end end reconnect(V,doorFarm,ex,ye)end else while findItem(blockId)>0 and findItem(seedId)<190 do while(bot:getTile(ex-1,ye)).fg==0 and(bot:getTile(ex-1,ye)).bg==0 do place(blockId,-1,0)sleep(delayPlace)reconnect(V,doorFarm,ex,ye)end while(bot:getTile(ex-1,ye)).fg~=0 or(bot:getTile(ex-1,ye)).bg~=0 do punch(-1,0)sleep(delayPunch)reconnect(V,doorFarm,ex,ye)end end end clear()sleep(100)if(bot:getLocal()).gems>=minimumGem then buy()sleep(100)warp(V,doorFarm)sleep(100)bot:autoCollect(3,true)sleep(100)botInfo("\070\097\114\109\105\110\103")sleep(100)end end end function scanBlock()local V={}for t,l in pairs(bot:getObjects())do if l.id==blockId then if V[l.id]then V[l.id].count=V[l.id].count+l.count else V[l.id]={id=l.id,count=l.count}end end end if V[blockId]then return V[blockId].count else return 0 end end function takeBlock(V)for t,l in pairs(bot:getObjects())do if l.id==blockId and#bot:getPath(round(l.pos.x/32),math.floor(l.pos.y/32))>0 then bot:findPath(round(l.pos.x/32),math.floor(l.pos.y/32))sleep(500)collect(2)sleep(1000)reconnect(V,doorFarm,round(l.pos.x/32),math.floor(l.pos.y/32))end if findItem(blockId)>=190 then break end end end function pnb2(V)if findItem(blockId)<tileNumber then takeBlock(V)sleep(100)end if findItem(blockId)>=tileNumber then botInfo("\066\114\101\097\107\105\110\103 \066\108\111\099\107\115")sleep(100)if not customPNB then ex=1 ye=math.floor((bot:getLocal()).pos.y/32)if ye>40 then ye=ye-10 elseif ye<11 then ye=ye+10 end if(bot:getTile(ex,ye)).fg~=0 and(bot:getTile(ex,ye)).fg~=seedId then ye=ye-1 end else ex=customX ye=customY end while math.floor((bot:getLocal()).pos.x/32)~=ex or math.floor((bot:getLocal()).pos.y/32)~=ye do bot:findPath(ex,ye)sleep(1500)end if tileNumber>1 then while findItem(blockId)>=tileNumber and findItem(seedId)<190 do while tilePlace(ex,ye)do for t,l in pairs(tileBreak)do if(bot:getTile(ex-1,ye+l)).fg==0 and(bot:getTile(ex-1,ye+l)).bg==0 then place(blockId,-1,l)sleep(delayPlace)reconnect(V,doorFarm,ex,ye)end end end while tilePunch(ex,ye)do for t,l in pairs(tileBreak)do if(bot:getTile(ex-1,ye+l)).fg~=0 or(bot:getTile(ex-1,ye+l)).bg~=0 then punch(-1,l)sleep(delayPunch)reconnect(V,doorFarm,ex,ye)end end end reconnect(V,doorFarm,ex,ye)end else while findItem(blockId)>0 and findItem(seedId)<190 do while(bot:getTile(ex-1,ye)).fg==0 and(bot:getTile(ex-1,ye)).bg==0 do place(blockId,-1,0)sleep(delayPlace)reconnect(V,doorFarm,ex,ye)end while(bot:getTile(ex-1,ye)).fg~=0 or(bot:getTile(ex-1,ye)).bg~=0 do punch(-1,0)sleep(delayPunch)reconnect(V,doorFarm,ex,ye)end end end clear()sleep(100)if(bot:getLocal()).gems>=minimumGem then buy()sleep(100)warp(V,doorFarm)sleep(100)bot:autoCollect(3,true)sleep(100)botInfo("\070\097\114\109\105\110\103")sleep(100)end end end function harvest(V)botInfo("\072\097\114\118\101\115\116\105\110\103 \084\114\101\101\115")sleep(100)countWorld(V)sleep(100)for t,l in ipairs(bot:getTiles({skip=true}))do if l.fg==seedId then if bot:canHarvest(l.x,l.y)and bot:hasAccessToTile(l.x,l.y)then bot:findPath(l.x,l.y)sleep(100)while(bot:getTile(l.x,l.y)).fg==seedId and(bot:canHarvest(l.x,l.y)and bot:hasAccessToTile(l.x,l.y))do punch(0,0)sleep(delayHarvest)reconnect(V,doorFarm,l.x,l.y)end end end if findItem(blockId)>=190 or findItem(seedId)>=190 then pnb(V)sleep(100)plant(V)sleep(100)end end pnb(V)sleep(100)plant2(V)sleep(100)if(bot:getLocal()).gems>=minimumGem then buy()sleep(100)end end function collect2(V)for t,l in pairs(bot:getObjects())do local U=round(l.pos.x/32)local B=math.floor(l.pos.y/32)if l.count<190 then if bot:isInside(U,B,V)then bot:collectObject(l.uid)end end end end function harvest2(V)botInfo("\072\097\114\118\101\115\116\105\110\103 \084\114\101\101\115")sleep(100)countWorld(V)sleep(100)for t,l in ipairs(bot:getTiles({skip=true}))do if l.fg==seedId then if bot:canHarvest(l.x,l.y)and bot:hasAccessToTile(l.x,l.y)then bot:findPath(l.x,l.y)sleep(100)bot:autoCollect(2,false)while(bot:getTile(l.x,l.y)).fg==seedId and(bot:canHarvest(l.x,l.y)and bot:hasAccessToTile(l.x,l.y))do punch(0,0)sleep(delayHarvest)collect2(3)reconnect(V,doorFarm,l.x,l.y)end end if findItem(blockId)>=190 then while findItem(blockId)>=190 and tileDrop1(l.x+1,l.y,findItem(blockId))do bot:sendPacket("\097\099\116\105\111\110\124\100\114\111\112\n\124\105\116\101\109\073\068\124"..blockId,2)sleep(1250)bot:sendPacket("\097\099\116\105\111\110\124\100\105\097\108\111\103\095\114\101\116\117\114\110\n\100\105\097\108\111\103\095\110\097\109\101\124\100\114\111\112\095\105\116\101\109\n\105\116\101\109\073\068\124"..(blockId..("\124\n\099\111\117\110\116\124"..findItem(blockId))),2)sleep(1250)reconnect(V,doorFarm,l.x,l.y)end end end if findItem(seedId)>=190 then storeSeed(V)sleep(100)end if(bot:getLocal()).level>=minimumLevel then bot:autoCollect(3,true)sleep(100)break end end if(bot:getLocal()).gems>=minimumGem then buy()sleep(100)end end function wear(V)pkt={}pkt.type=10 pkt.int_data=V bot:sendPacketRaw(pkt)end function takePickaxe()if autoPickaxe then warp(pickStorage,doorPick)sleep(100)bot:autoCollect(3,false)sleep(100)for V,t in pairs(bot:getObjects())do if t.id==98 then bot:findPath(round(t.pos.x/32)-1,math.floor(t.pos.y/32))sleep(1000)collect(2,98)sleep(1000)reconnect(pickStorage,doorPick)end if findItem(98)>1 then while findItem(98)>1 and tileDrop1(round(t.pos.x/32),math.floor(t.pos.y/32),findItem(98)-1)do bot:sendPacket("\097\099\116\105\111\110\124\100\114\111\112\n\124\105\116\101\109\073\068\124\057\056",2)sleep(1250)bot:sendPacket("\097\099\116\105\111\110\124\100\105\097\108\111\103\095\114\101\116\117\114\110\n\100\105\097\108\111\103\095\110\097\109\101\124\100\114\111\112\095\105\116\101\109\n\105\116\101\109\073\068\124\057\056\124\n\099\111\117\110\116\124"..findItem(98)-1,2)sleep(1250)reconnect(pickStorage,doorPick)end end if findItem(98)==1 then break end end wear(98)sleep(2000)end end takePickaxe()while true do leveledUp=false for V,t in pairs(worlds)do sleep(100)warp(t,doorFarm)sleep(100)bot:autoCollect(3,true)sleep(100)bl(t)sleep(100)countWorld(t)sleep(100)botInfo("\083\116\097\114\116\105\110\103 "..t)sleep(100)tt=os.time()local l=scanBlock()>0 local U=(bot:getLocal()).level>=minimumLevel if l and U then plant3(t)sleep(100)while scanBlock()>0 do pnb2(t)sleep(100)if findItem(seedId)>=100 then storeSeed(t)sleep(100)end end elseif not l and U then harvest(t)sleep(100)elseif not U then harvest2(t)sleep(100)if U then leveledUp=true break end end tt=os.time()-tt botInfo("\070\105\110\105\115\104\101\100 "..t)sleep(100)if start<stop then start=start+1 else if restartTimer then waktu={}tree={}end start=1 end end if not looping and not leveledUp then botInfo("\070\105\110\105\115\104\101\100 \065\108\108 \087\111\114\108\100\044 \082\101\109\111\118\105\110\103 \066\111\116\033")sleep(100)bot:remove()break end end
