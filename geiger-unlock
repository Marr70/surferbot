ids=k.id,jumlah=k.count})sleep(50)end end function is_included(w,k)for k,Z in pairs(k)do if w==Z.ids then return true end end return false end function is_changed(w,k,Z)for Z,f in pairs(Z)do if w==f.ids then if k==f.jumlah then return false else return true end end end end function count_changed(w,k)for k,Z in pairs(k)do if w==Z.ids then return bot:getItemCount(w)-Z.jumlah end end end function get_found_item()for w,k in pairs(bot:getInventory())do if k.id==2286 then sleep(50)else if not is_included(k.id,goods)or is_changed(k.id,k.count,goods)then local w=(getItemInfo(k.id)).name local Z=count_changed(k.id,goods)or bot:getItemCount(k.id)return string.format("\070\111\117\110\100 \073\116\101\109\058  \037\115 \040\120\037\115\041",w,Z)end end end return"\099\097\110\'\116 \100\101\116\101\114\109\105\110\101 \102\111\117\110\100 \105\116\101\109\115"end function wear(w)local k={}k.type=10 k.int_data=w bot:sendPacketRaw(k)end function getPosX()return math.floor((bot:getLocal()).pos.x/32)end function getPosY()return math.floor((bot:getLocal()).pos.y/32)end function rapid_up()local w=midX local k=midY if not bot:isEquipped(2204)then return true end while math.floor((bot:getLocal()).pos.y/32)>=0 do while bot:isEquipped(2204)do if get_signals()==4 then if bot:findPath(getPosX()+1,getPosY())then sleep(move_delay)end if estatus()~=Connected then return end else break end if not bot:isEquipped(2204)then return true end end if bot:findPath(w,k)then sleep(move_delay)end if estatus()~=Connected then return end while bot:isEquipped(2204)do if get_signals()==4 then if bot:findPath(getPosX()-1,getPosY())then sleep(move_delay)end if estatus()~=Connected then return end else break end if not bot:isEquipped(2204)then return true end end if math.floor((bot:getLocal()).pos.y/32)~=0 then k=k-1 if bot:findPath(w,k)then sleep(1500)if get_signals()~=4 then return false end end if estatus()~=Connected then return end else return false end end end function rapid_down()local w=midX local k=mi