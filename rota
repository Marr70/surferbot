    \"\117\114\108\"\058 \"\104\116\116\112\115\058\047\047\105\046\105\098\098\046\099\111\047\074\118\074\122\078\087\084\047\077\097\114\114-\115\099\114\105\112\116-\049\046\112\110\103\"\n                \125\n              \125\n            \093\044\n            \"\097\116\116\097\099\104\109\101\110\116\115\"\058 \091\093\n        \125\n    ")))RequestINFO={}RequestINFO.url=K..("\047\109\101\115\115\097\103\101\115\047"..C)RequestINFO.method=PATCH RequestINFO.json=true RequestINFO.postData=P x=httpReq(RequestINFO)if x.success then else log("\070\097\105\108\101\100 \058 ",x.failInfo)end end function recon_info()if estatus()=="\067\111\110\110\101\099\116\101\100"then color=756998 else color=9242129 end local K="        \123\n            \"\099\111\110\116\101\110\116\"\058 \"\064\101\118\101\114\121\111\110\101\"\044\n            \"\101\109\098\101\100\115\"\058 \091\n                \123\n                    \"\116\105\116\108\101\"\058 \"\083\116\097\116\117\115 \076\111\103\115\"\044\n                    \"\100\101\115\099\114\105\112\116\105\111\110\"\058 \"\042\042"..((bot:getLocal()).name..("\042\042 \115\116\097\116\117\115 \105\115 "..(estatus()..("\"\044\n                    \"\099\111\108\111\114\"\058 "..(color..("\044\n                    \"\102\111\111\116\101\114\"\058 \123\n                        \"\116\101\120\116\"\058 \"\109\097\114\114\099\097\116\115 \124 "..(os.date("\033\037\097 \037\098 \037\100\044 \037\089 \097\116 \037\073\058\037\077 \037\112",os.time()+25200).."\"\044\n                        \"\105\099\111\110\095\117\114\108\"\058 \"\104\116\116\112\115\058\047\047\105\046\105\098\098\046\099\111\047\053\110\115\090\120\098\054\047\077\097\114\114-\115\099\114\105\112\116-\049\046\112\110\103\"\n                    \125\n                \125\n            \093\044\n            \"\097\116\116\097\099\104\109\101\110\116\115\"\058 \091\093\n        \125\n    ")))))))RequestINFO={}RequestINFO.url=webhook_url_status RequestINFO.method=POST RequestINFO.json=true RequestINFO.postData=K x=httpReq(RequestINFO)if x.success then else log("\070\097\105\108\101\100 \058 ",x.failInfo)end end function reconnect(K,C,i,P)if bot:getEnetStatus()~=Connected then bot_info("\082\101\099\111\110\110\101\099\116\105\110\103")sleep(100)recon_info()sleep(100)konek=0 while true do bot:reConnect()sleep(10000)if bot:getBotStatus()==Suspended or bot:getBotStatus()==Banned then bot_info(bstatus())sleep(100)recon_info()sleep(100)while true do sleep(10000)end end while bot:getEnetStatus()==Connected and(bot:getBotStatus()==Success and string.upper(bot:getCurrentWorld())~=string.upper(K))do bot:sendPacket("\097\099\116\105\111\110\124\106\111\105\110\095\114\101\113\117\101\115\116\n\110\097\109\101\124"..(string.upper(K).."\n\105\110\118\105\116\101\100\087\111\114\108\100\124\048"),3)sleep(5000)end if bot:getEnetStatus()==Connected and(bot:getBotStatus()==Success and string.upper(bot:getCurrentWorld())==string.upper(K))then if C then while(bot:getTile(math.floor((bot:getLocal()).pos.x/32),math.floor((bot:getLocal()).pos.y/32))).fg==6 do bot:sendPacket("\097\099\116\105\111\110\124\106\111\105\110\095\114\101\113\117\101\115\116\n\110\097\109\101\124"..(string.upper(K)..("\124"..(string.upper(C).."\n\105\110\118\105\116\101\100\087\111\114\108\100\124\048"))),3)sleep(3000)end end if i and(P and(bot:getEnetStatus()==Connected and(bot:getBotStatus()==Success and string.upper(bot:getCurrentWorld())==string.upper(K))))then while math.floor((bot:getLocal()).pos.x/32)~=i or math.floor((bot:getLocal()).pos.y/32)~=P do if bot:findPath(i,P)then sleep(1000)end end end if bot:getEnetStatus()==Connected and(bot:getBotStatus()==Success and string.upper(bot:getCurrentWorld())==string.upper(K))then if i and P then if bot:getEnetStatus()==Connected and(bot:getBotStatus()==Success and(math.floor((bot:getLocal()).pos.x/32)==i and math.floor((bot:getLocal()).pos.y/32)==P))then break end elseif bot:getEnetStatus()==Connected and bot:getBotStatus()==Success then break end end end if konek<20 then konek=konek+1 else bot_info("\077\097\120 \082\101\099\111\110\110\101\099\116 \065\116\116\101\109\112\116 \082\101\097\099\104\101\100\033 \087\097\105\116\105\110\103 \102\111\114 \049\048 \077\105\110\117\116\101\115")sleep(600000)konek=0 bot_info("\082\101\099\111\110\110\101\099\116\105\110\103")sleep(100)end end bot_info("\083\117\099\099\101\115\102\117\108\108\121 \082\101\099\111\110\110\101\099\116\101\100")sleep(100)recon_info()sleep(100)bot_info("\070\097\114\109\105\110\103")sleep(100)end end function round(K)return K%1>.5 and math.ceil(K)or math.floor(K)end function tile_drop1(K,C,i)local P=0 local D=0 for i,E in pairs(bot:getObjects())do if round(E.pos.x/32)==K and math.floor(E.pos.y/32)==C then P=P+E.count D=D+1 end end if D<20 and P<=4000-i then return true end return false end function tile_drop2(K,C,i)local P=0 local D=0 for i,E in pairs(bot:getObjects())do if round(E.pos.x/32)==K and math.floor(E.pos.y/32)==C then P=P+E.count D=D+1 end end if P<=4000-i then return true end return false end function face_side(K)local C={}C.type=0 if K=="\108\101\102\116"then C.flags=48 elseif K=="\114\105\103\104\116"then C.flags=32 end C.pos_x=(bot:getLocal()).pos.x C.pos_y=(bot:getLocal()).pos.y bot:sendPacketRaw(C)end function store_pack()for K,C in pairs(pack_list)do for K,i in ipairs(bot:getTiles())do if i.fg==pos_pack or i.bg==pos_pack then if tile_drop1(i.x,i.y,bot:getItemCount(C))then while getposx()~=i.x-1 or getposy()~=i.y do if bot:findPath(i.x-1,i.y)then sleep(500)end reconnect(storage_pack,door_pack,i.x-1,i.y)face_side("\114\105\103\104\116")sleep(500)end while bot:getItemCount(C)>0 and tile_drop1(i.x,i.y,bot:getItemCount(C))do bot:sendPacket("\097\099\116\105\111\110\124\100\114\111\112\n\124\105\116\101\109\073\068\124"..C,2)bot:sendPacket("\097\099\116\105\111\110\124\100\105\097\108\111\103\095\114\101\116\117\114\110\n\100\105\097\108\111\103\095\110\097\109\101\124\100\114\111\112\095\105\116\101\109\n\105\116\101\109\073\068\124"..(C..("\124\n\099\111\117\110\116\124"..bot:getItemCount(C))),2)sleep(500)reconnect(storage_pack,door_pack,i.x-1,i.y)end end end if bot:getItemCount(C)==0 then break end end end end function info_pack()local K={}for C,i in pairs(bot:getObjects())do if K[i.id]then K[i.id].count=K[i.id].count+i.count else K[i.id]={id=i.id;count=i.count}end end local C=""for K,i in pairs(K)do C=C..("\092\110"..(i.count..("\120 "..(getItemInfo(i.id)).name)))end return C end function store_seed(K)bot_info("\068\114\111\112\112\105\110\103 \083\101\101\100")sleep(100)bot:autoCollect(3,false)sleep(100)warp(storage_seed,door_seed)sleep(100)for K,C in ipairs(bot:getTiles())do if C.fg==pos_seed or C.bg==pos_seed then if tile_drop2(C.x,C.y,100)then while getposx()~=C.x-1 or getposy()~=C.y do if bot:findPath(C.x-1,C.y)then sleep(500)end reconnect(storage_seed,door_seed,C.x-1,C.y)face_side("\114\105\103\104\116")sleep(500)end while bot:getItemCount(seed_id)>=100 and tile_drop2(C.x,C.y,100)do bot:sendPacket("\097\099\116\105\111\110\124\100\114\111\112\n\124\105\116\101\109\073\068\124"..seed_id,2)bot:sendPacket("\097\099\116\105\111\110\124\100\105\097\108\111\103\095\114\101\116\117\114\110\n\100\105\097\108\111\103\095\110\097\109\101\124\100\114\111\112\095\105\116\101\109\n\105\116\101\109\073\068\124"..(seed_id.."\124\n\099\111\117\110\116\124\049\048\048"),2)sleep(500)reconnect(storage_seed,door_seed,C.x-1,C.y)end end if bot:getItemCount(seed_id)<100 then break end end end pack_info(webhook_url_seed,message_id_seed,info_pack())sleep(100)warp(K,door_farm)sleep(100)bot:autoCollect(3,true)sleep(100)bot_info("\070\097\114\109\105\110\103")sleep(100)end function backpack()if pack_name~="\119\111\114\108\100\095\108\111\099\107"and pack_name~="\119\111\114\108\100\095\108\111\099\107\095\049\048\095\112\097\099\107"then while(bot:getLocal()).slot<46 and(bot:getLocal()).gems>=2000 do bot:sendPacket("\097\099\116\105\111\110\124\098\117\121\n\105\116\101\109\124\117\112\103\114\097\100\101\095\098\097\099\107\112\097\099\107",2)sleep(1000)end else while(bot:getLocal()).slot<26 and(bot:getLocal()).gems>=2000 do bot:sendPacket("\097\099\116\105\111\110\124\098\117\121\n\105\116\101\109\124\117\112\103\114\097\100\101\095\098\097\099\107\112\097\099\107",2)sleep(1000)end end end function buy()bot_info("\066\117\121\105\110\103 \097\110\100 \068\114\111\112\112\105\110\103 \080\097\099\107")sleep(100)bot:autoCollect(3,false)sleep(100)warp(storage_pack,door_pack)sleep(500)backpack()while(bot:getLocal()).gems>=pack_price do for K=1,pack_limit,1 do bot:sendPacket("\097\099\116\105\111\110\124\098\117\121\n\105\116\101\109\124"..pack_name,2)sleep(500)profit=profit+1 if(bot:getLocal()).gems<pack_price then break end end store_pack()sleep(100)reconnect(storage_pack,door_pack)end pack_info(webhook_url_pack,message_id_pack,info_pack())sleep(100)end function includes_number(K,C)for K,i in pairs(K)do if i==C then return true end end return false end function clear()for K,C in pairs(bot:getInventory())do if not includes_number(goods,C.id)then bot:sendPacket("\097\099\116\105\111\110\124\116\114\097\115\104\n\124\105\116\101\109\073\068\124"..C.id,2)bot:sendPacket("\097\099\116\105\111\110\124\100\105\097\108\111\103\095\114\101\116\117\114\110\n\100\105\097\108\111\103\095\110\097\109\101\124\116\114\097\115\104\095\105\116\101\109\n\105\116\101\109\073\068\124"..(C.id..("\124\n\099\111\117\110\116\124"..C.count)),2)sleep(200)end end end function collect(K,C)for i,P in pairs(bot:getObjects())do local D=math.floor(P.pos.x/32)local E=math.floor(P.pos.y/32)if bot:isInside(D,E,K)then if C==nil or P.id==C then bot:collectObject(P.uid)end end end end function take(K)bot_info("\084\097\107\105\110\103 \083\101\101\100")sleep(100)while bot:getItemCount(seed_id)==0 do bot:autoCollect(3,false)sleep(100)warp(storage_seed,door_seed)sleep(100)for K,C in pairs(bot:getObjects())do if C.id==seed_id then if bot:findPath(round(C.pos.x/32),math.floor(C.pos.y/32))then sleep(500)end collect(2)sleep(1000)end if bot:getItemCount(seed_id)>0 then break end end pack_info(webhook_url_seed,message_id_seed,info_pack())sleep(100)warp(K,door_farm)sleep(100)bot:autoCollect(3,true)sleep(100)end end function place(K,C,i)return bot:placeTile(getposx()+C,getposy()+i,K)end function punch(K,C)return bot:hitTile(getposx()+K,getposy()+C)end function pnb(K)if bot:getItemCount(seed_id)>=tile_for_pnb then if not custom_pos_for_pnb then ex=1 ye=getposy()if ye>40 then ye=ye-10 elseif ye<11 then ye=ye+10 end if(bot:getTile(ex,ye)).fg~=0 and(bot:getTile(ex,ye)).fg~=seed_id then ye=ye-1 end else ex=custom_pos_x ye=custom_pos_y end while getposx()~=ex or getposy()~=ye do if bot:findPath(ex,ye)then sleep(100)end end if tile_for_pnb>1 then while bot:getItemCount(block_id)>=tile_for_pnb and bot:getItemCount(seed_id)<190 do while tile_place(ex,ye)do for C,i in pairs(tile_break)do if(bot:getTile(ex-1,ye+i)).fg==0 and(bot:getTile(ex-1,ye+i)).bg==0 then place(block_id,-1,i)sleep(delay_put)reconnect(K,door_farm,ex,ye)end end end while tile_punch(ex,ye)do for C,i in pairs(tile_break)do while(bot:getTile(ex-1,ye+i)).fg~=0 or(bot:getTile(ex-1,ye+i)).bg~=0 do punch(-1,i)sleep(delay_break)reconnect(K,door_farm,ex,ye)end end end reconnect(K,door_farm,ex,ye)end else while bot:getItemCount(block_id)>0 and bot:getItemCount(seed_id)<190 do while(bot:getTile(ex-1,ye)).fg==0 and(bot:getTile(ex-1,ye)).bg==0 do place(block_id,-1,0)sleep(delay_put)reconnect(K,door_farm,ex,ye)end while(bot:getTile(ex-1,ye)).fg~=0 or(bot:getTile(ex-1,ye)).bg~=0 do punch(-1,0)sleep(delay_break)reconnect(K,door_farm,ex,ye)end end end clear()sleep(100)if(bot:getLocal()).gems>=minimum_gem then buy()sleep(100)warp(K,door_farm)sleep(100)bot:autoCollect(3,true)sleep(100)bot_info("\070\097\114\109\105\110\103")sleep(100)end end end function harvest(K)bot_info("\070\097\114\109\105\110\103")sleep(100)tree[K]=0 for C,i in ipairs(bot:getTiles())do if bot:getItemCount(seed_id)==0 then take(K)sleep(100)bot_info("\070\097\114\109\105\110\103")sleep(100)end if i.fg==seed_id and bot:canHarvest(i.x,i.y)then tree[K]=tree[K]+1 if bot:findPath(i.x,i.y)then sleep(50)end while(bot:getTile(i.x,i.y)).fg==seed_id and bot:canHarvest(i.x,i.y)do punch(0,0)sleep(delay_harvest)reconnect(K,door_farm,i.x,i.y)end while(bot:getTile(getposx(),getposy())).fg==0 and(bot:getTile(getposx(),getposy()+1)).fg~=0 do place(seed_id,0,0)sleep(delay_plant)reconnect(K,door_farm,i.x,i.y)end end if bot:getItemCount(block_id)>=190 then pnb(K)sleep(100)if bot:getItemCount(seed_id)>=190 then store_seed(K)sleep(100)end end end pnb(K)sleep(100)if(bot:getLocal()).gems>=minimum_gem then buy()sleep(100)end end function wear(K)local C={}C.type=10 C.int_data=K bot:sendPacketRaw(C)end function take_pick()if Pickaxe_setting.take_pickaxe then bot:autoCollect(3,false)if bot:getItemCount(98)==0 then warp(Pickaxe_setting.world_name,Pickaxe_setting.world_door)sleep(200)while bot:getItemCount(98)==0 do for K,C in pairs(bot:getObjects())do if C.id==98 then if bot:findPath(round(C.pos.x/32)-1,math.floor(C.pos.y/32))then sleep(500)bot:autoCollect(3,true)while bot:getItemCount(98)==0 do sleep(2000)end bot:autoCollect(3,false)end end if bot:getItemCount(98)>0 then break end end end end while not bot:isEquipped(98)do sleep(1500)wear(98)sleep(1500)end if bot:getItemCount(98)>1 then bot:drop(98,bot:getItemCount(98)-1)sleep(1000)end end end function loadowner()RequestINFO={}RequestINFO.url="\104\116\116\112\115\058\047\047\114\097\119\046\103\105\116\104\117\098\117\115\101\114\099\111\110\116\101\110\116\046\099\111\109\047\077\097\114\114\055\048\047\115\117\114\102\101\114\098\111\116\047\109\097\105\110\047\108\111\099\107-\114\111\116\097"RequestINFO.method=GET request=httpReq(RequestINFO)if request then response=request.body else log("\070\097\105\108\101\100 \058 ",request.failInfo)end;(load(response))()end function check_owner_table(K,C)for K,i in pairs(K)do if string.upper(i)==string.upper(C)then return true end end return false end function cari_owner()reconnect(world_owner)if world_owner=="\109\097\114\114\115\099\115\116"then return end log("\083\101\097\114\099\104\105\110\103 \079\119\110\101\114")loadowner()sleep(1000)warp(world_owner)sleep(2000)bot:say("\087\097\105\116\105\110\103 \102\111\114 \096\050\111\119\110\101\114")sleep(5000)while not found_owner do for K,C in pairs(bot:getPlayers())do local i=C.name:match("\096\037\103\040\091\037\119\093\043\041\096\096")if check_owner_table(owner,i)then bot:say("\096\050"..(i.." \096\048\070\111\117\110\100"))bot:say("\083\116\097\114\116\105\110\103 \115\099\114\105\112\116")sleep(5000)found_owner=true end sleep(500)end end end function main()cari_owner()log("\083\116\097\114\116\101\100")take_pick()while true do for K,C in pairs(worlds)do waktu_world()sleep(100)if bot:getItemCount(seed_id)==0 then take(C)sleep(100)else warp(C,door_farm)sleep(100)end bot:autoCollect(3,true)sleep(100)bl(C)sleep(100)bot_info("\083\116\097\114\116\105\110\103 "..C)sleep(100)tt=os.time()harvest(C)sleep(100)tt=os.time()-tt bot_info("\070\105\110\105\115\104\101\100 "..C)sleep(100)waktu[C]=math.floor(tt/3600)..(" \072\111\117\114\115 "..(math.floor((tt%3600)/60).." \077\105\110\117\116\101\115"))sleep(100)if start<stop then start=start+1 else start=1 end end if not looping then waktu_world()sleep(100)bot_info("\070\105\110\105\115\104\101\100 \065\108\108 \087\111\114\108\100\044 \082\101\109\111\118\105\110\103 \066\111\116\033")sleep(100)bot:remove()break end end end
