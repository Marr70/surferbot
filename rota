bot=getBot()worlds={}tile_break={}tree={}waktu={}fossil={}str_waktu=""profit=0 list_now=1 t=os.time()found_owner=false storage_seed=Storage_setting.storage_seed door_seed=Storage_setting.door_seed pos_seed=Storage_setting.pos_seed storage_pack=Storage_setting.storage_pack door_pack=Storage_setting.door_pack pos_pack=Storage_setting.pos_pack webhook_url_pack=Webhook_setting.webhook_url_pack message_id_pack=Webhook_setting.message_id_pack webhook_url_seed=Webhook_setting.webhook_url_seed message_id_seed=Webhook_setting.message_id_seed webhook_url_status=Webhook_setting.webhook_url_status delay_harvest=Delay_setting.delay_harvest delay_plant=Delay_setting.delay_plant delay_break=Delay_setting.delay_break delay_put=Delay_setting.delay_put tile_for_pnb=PNB_setting.tile_for_pnb custom_pos_for_pnb=PNB_setting.custom_pos_for_pnb custom_pos_x=PNB_setting.custom_pos_x custom_pos_y=PNB_setting.custom_pos_y pack=Pack_setting.pack pack_list=Pack_setting.pack_list pack_name=Pack_setting.pack_name minimum_gem=Pack_setting.minimum_gem pack_price=Pack_setting.pack_price pack_limit=Pack_setting.pack_limit goods=Other_setting.trash_whitelist looping=Other_setting.looping custom_show_world=Other_setting.custom_show_world custom_show_world_amount=Other_setting.custom_show_world_amount if use_guest then for a,e in pairs(Bots)do if e.guest-1==bot:getIndex()then start=e.start_from stop=#e.world_list door_farm=e.door_farm webhook_url=e.webhook_url message_id=e.message_id world_list=e.world_list total_list=#e.world_list end end else start=Bots[string.upper((bot:getLocal()).name)].start_from stop=#Bots[string.upper((bot:getLocal()).name)].world_list door_farm=Bots[string.upper((bot:getLocal()).name)].door_farm webhook_url=Bots[string.upper((bot:getLocal()).name)].webhook_url message_id=Bots[string.upper((bot:getLocal()).name)].message_id world_list=Bots[string.upper((bot:getLocal()).name)].world_list total_list=#Bots[string.upper((bot:getLocal()).name)].world_list end if Event_setting.enabled then for a,e in pairs(Event_setting.list)do table.insert(goods,e)end end for a=start,stop,1 do table.insert(worlds,world_list[a])end if looping then for a=0,start-1,1 do table.insert(worlds,world_list[a])end end for a,e in pairs(pack_list)do table.insert(goods,e)end for a=math.floor(tile_for_pnb/2),1,-1 do a=a*-1 table.insert(tile_break,a)end for a=0,math.ceil(tile_for_pnb/2)-1,1 do table.insert(tile_break,a)end if custom_show_world_amount-1>=#world_list then custom_show_world=false end function rest()if Rest_setting.enabled then for a,e in pairs(Rest_setting.schedule)do local K=e.from:gsub("\037\046","")K=tonumber(K)local J=e.to:gsub("\037\046","")J=tonumber(J)local R=tonumber(os.date("\033\037\072\046\037\077",os.time()+(Rest_setting.time_difference*60)*60))*100 if J<K then J=J+2400 end if R>=K and R<J then local a=J-R local K,q=math.floor(a/100),a%100 a=K*60+q sleep(100)bot_info("\066\111\116 \082\101\115\116 \117\110\116\105\108 "..e.to)sleep(500)bot:setBool("\097\117\116\111\082\101\099\111\110\110\101\099\116",false)sleep(500)bot:disconnect()sleep(1000)recon_info()sleep(a*60000)end end end end function logs(a)log(string.format("\091\037\115\093 \037\115 \n",(bot:getLocal()).name,a))end function getposx()return math.floor((bot:getLocal()).pos.x/32)end function getposy()return math.floor((bot:getLocal()).pos.y/32)end function bl(a)fossil[a]=0 for e,K in ipairs(bot:getTiles())do if K.fg==3918 then fossil[a]=fossil[a]+1 end end end function tile_punch(a,e)for K,J in pairs(tile_break)do if(bot:getTile(a-1,e+J)).fg~=0 or(bot:getTile(a-1,e+J)).bg~=0 then return true end end return false end function tile_place(a,e)for K,J in pairs(tile_break)do if(bot:getTile(a-1,e+J)).fg==0 and(bot:getTile(a-1,e+J)).bg==0 then return true end end return false end function warp(a,e)while string.upper((bot:getWorld()).name)~=string.upper(a)do bot:sendPacket("\097\099\116\105\111\110\124\106\111\105\110\095\114\101\113\117\101\115\116\n\110\097\109\101\124"..(string.upper(a).."\n\105\110\118\105\116\101\100\087\111\114\108\100\124\048"),3)sleep(6000)end if e==""or e==nil then return end while(bot:getTile(math.floor((bot:getLocal()).pos.x/32),math.floor((bot:getLocal()).pos.y/32))).fg==6 do bot:sendPacket("\097\099\116\105\111\110\124\106\111\105\110\095\114\101\113\117\101\115\116\n\110\097\109\101\124"..(string.upper(a)..("\124"..(string.upper(e).."\n\105\110\118\105\116\101\100\087\111\114\108\100\124\048"))),3)sleep(6000)end end function waktu_world()str_waktu=""if custom_show_world then for a=custom_show_world_amount,1,-1 do new_list=list_now-a if new_list<=0 then new_list=new_list+total_list end str_waktu=str_waktu..("\092\110"..(string.upper(world_list[new_list])..(" \040 "..((waktu[world_list[new_list]]or"\063")..(" \124 "..((tree[world_list[new_list]]or"\063").." \041"))))))end else for a,e in pairs(world_list)do str_waktu=str_waktu..("\092\110"..(string.upper(e)..(" \040 "..((waktu[e]or"\063")..(" \124 "..((tree[e]or"\063").." \041"))))))end end end function bstatus()local a=bot:getBotStatus()if a==Success then return"\083\117\099\099\101\115\115"elseif a==Disconnected then return"\068\105\115\099\111\110\110\101\099\116\101\100"elseif a==OnSendToServer then return"\079\110 \083\101\110\100 \084\111 \083\101\114\118\101\114"elseif a==Banned then return"\066\097\110\110\101\100"elseif a==Suspended then return"\083\117\115\112\101\110\100\101\100"elseif a==Update_Required then return"\085\112\100\097\116\101 \082\101\113\117\105\114\101\100"elseif a==Maintenance then return"\077\097\105\110\116\101\110\097\110\099\101"elseif a==SERVER_OVERLOADED then return"\083\101\118\101\114 \079\118\101\114\108\111\097\100\101\100"else return 0 end end function estatus()local a=bot:getEnetStatus()if a==Connected then return"\067\111\110\110\101\099\116\101\100"elseif a==Disconnected then return"\068\105\115\099\111\110\110\101\099\116\101\100"elseif a==ShadowBan then return"\083\104\097\100\111\119 \066\097\110"else return 0 end end function format_number(a)local e=tostring(a)local K=string.len(e)for a=K-3,1,-3 do e=string.sub(e,1,a)..("\046"..string.sub(e,a+1))end return e end function bot_info(a)te=os.time()-t fossill=fossil[bot:getCurrentWorld()]or 0 local e="        \123\n            \"\099\111\110\116\101\110\116\"\058 \110\117\108\108\044\n            \"\101\109\098\101\100\115\"\058 \091\n              \123\n                \"\116\105\116\108\101\"\058 \"\042\042\082\111\116\097\116\105\111\110 \066\121 \077\097\114\114\042\042\"\044\n                \"\099\111\108\111\114\"\058 \048\044\n                \"\102\105\101\108\100\115\"\058 \091\n                  \123\n                    \"\110\097\109\101\"\058 \"\095\095\066\079\084 \073\078\070\079\082\077\065\084\073\079\078\095\095\"\044\n                    \"\118\097\108\117\101\"\058 \"\060\058\105\099\111\110\095\105\110\102\111\058\049\049\051\056\052\056\049\052\054\049\054\055\057\051\055\052\052\051\055\062 \042\042\084\097\115\107\042\042 \058 "..(a..("\092\110\060\058\105\110\102\111\114\109\097\116\105\111\110\058\049\049\051\056\052\056\049\055\054\056\048\052\051\057\050\053\053\050\052\062 \042\042\066\111\116 \078\097\109\101\042\042 \058 "..((bot:getLocal()).name..(" \042\042\040"..((bot:getLocal()).level..("\041\042\042\092\110\060\058\111\102\102\108\105\110\101\095\100\101\115\107\116\111\112\058\049\049\051\056\052\056\051\054\054\055\056\053\051\057\049\052\049\054\050\062 \042\042\066\111\116 \083\116\097\116\117\115\042\042 \058 "..(bstatus()..("\092\110\060\058\111\102\102\108\105\110\101\095\119\101\098\058\049\049\051\056\052\056\051\057\052\053\048\050\054\048\057\055\050\052\050\062 \042\042\069\078\101\116 \083\116\097\116\117\115\042\042 \058 "..(estatus()..("\092\110\060\058\115\105\108\118\101\114\058\049\049\051\056\052\056\052\057\050\051\056\055\053\057\057\057\055\055\052\062 \042\042\071\101\109\115\042\042 \058 "..(format_number((bot:getLocal()).gems)..("\092\110\060\058\103\108\111\098\101\115\058\049\049\051\056\053\048\051\055\057\056\048\057\055\049\056\054\056\056\055\062 \042\042\087\111\114\108\100 \042\042 \058 "..(bot:getCurrentWorld()..(" \042\042\040"..(fossill..("\041\042\042\092\110 \060\058\116\111\100\111\095\108\105\115\116\058\049\049\051\056\052\057\056\049\049\050\048\052\050\056\051\050\048\049\051\062 \042\042\087\111\114\108\100 \079\114\100\101\114\042\042 \058 "..(start..(" \047 "..(stop..("\"\044\n                    \"\105\110\108\105\110\101\"\058 \116\114\117\101\n                  \125\044\n                  \123\n                    \"\110\097\109\101\"\058 \"\095\095\080\065\067\075 \073\078\070\079\082\077\065\084\073\079\078\095\095\"\044\n                    \"\118\097\108\117\101\"\058 \"\060\058\083\104\111\112\112\105\110\103\099\097\114\116\058\049\049\051\056\052\057\053\057\048\049\050\049\054\056\048\056\057\057\048\062 \042\042\080\097\099\107 \078\097\109\101\042\042 \058 "..(pack..("\092\110\060\058\100\111\108\108\097\114\115\058\049\049\051\056\052\057\054\052\048\055\050\055\054\051\054\051\057\048\054\062 \042\042\080\114\111\102\105\116\042\042 \058 "..(profit..(" "..(pack..("\"\044\n                    \"\105\110\108\105\110\101\"\058 \116\114\117\101\n                  \125\044\n                  \123\n                    \"\110\097\109\101\"\058 \"\095\095\087\079\082\076\068 \073\078\070\079\082\077\065\084\073\079\078\095\095\"\044\n                    \"\118\097\108\117\101\"\058 \""..(str_waktu..("\"\n                  \125\n                \093\044\n                \"\102\111\111\116\101\114\"\058 \123\n                  \"\116\101\120\116\"\058 \"\066\111\116 \085\112\116\105\109\101\058 "..(math.floor(te/86400)..(" \068\097\121 "..(math.floor((te%86400)/3600)..(" \072\111\117\114 "..(math.floor(((te%86400)%3600)/60)..(" \077\105\110\117\116\101 \092\110\109\097\114\114\099\097\116\115 \124 "..(os.date("\033\037\097 \037\098 \037\100\044 \037\089 \097\116 \037\073\058\037\077 \037\112",os.time()+25200).."\"\044\n                  \"\105\099\111\110\095\117\114\108\"\058 \"\104\116\116\112\115\058\047\047\105\046\105\098\098\046\099\111\047\053\110\115\090\120\098\054\047\077\097\114\114-\115\099\114\105\112\116-\049\046\112\110\103\"\n                \125\044\n                \"\116\104\117\109\098\110\097\105\108\"\058 \123\n                  \"\117\114\108\"\058 \"\104\116\116\112\115\058\047\047\105\046\105\098\098\046\099\111\047\074\118\074\122\078\087\084\047\077\097\114\114-\115\099\114\105\112\116-\049\046\112\110\103\"\n                \125\n              \125\n            \093\n        \125\n    ")))))))))))))))))))))))))))))))))))RequestINFO={}RequestINFO.url=webhook_url..("\047\109\101\115\115\097\103\101\115\047"..message_id)RequestINFO.method=PATCH RequestINFO.json=true RequestINFO.postData=e x=httpReq(RequestINFO)if x.success then else log("\070\097\105\108\101\100 \058 ",x.failInfo)end end function pack_info(a,e,K)local J="        \123\n            \"\099\111\110\116\101\110\116\"\058 \110\117\108\108\044\n            \"\101\109\098\101\100\115\"\058 \091\n              \123\n                \"\116\105\116\108\101\"\058 \"\060\058\083\116\097\114\080\105\101\099\101\058\049\049\051\056\054\050\050\052\050\057\055\057\049\053\051\057\050\049\048\062 \080\097\099\107 \038 \083\101\101\100 \073\110\102\111\114\109\097\116\105\111\110 \060\058\083\116\097\114\080\105\101\099\101\058\049\049\051\056\054\050\050\052\050\057\055\057\049\053\051\057\050\049\048\062\"\044\n                \"\099\111\108\111\114\"\058 \049\051\056\057\055\049\054\044\n                \"\102\105\101\108\100\115\"\058 \091\n                  \123\n                    \"\110\097\109\101\"\058 \"\068\114\111\112\112\101\100 \073\116\101\109\115\"\044\n                    \"\118\097\108\117\101\"\058 \""..(K..("\"\n                  \125\n                \093\044\n                \"\102\111\111\116\101\114\"\058 \123\n                    \"\116\101\120\116\"\058 \"\109\097\114\114\099\097\116\115 \124 "..(os.date("\033\037\097 \037\098 \037\100\044 \037\089 \097\116 \037\073\058\037\077 \037\112",os.time()+25200).."\"\044\n                    \"\105\099\111\110\095\117\114\108\"\058 \"\104\116\116\112\115\058\047\047\105\046\105\098\098\046\099\111\047\053\110\115\090\120\098\054\047\077\097\114\114-\115\099\114\105\112\116-\049\046\112\110\103\"\n                  \125\044\n                \"\116\104\117\109\098\110\097\105\108\"\058 \123\n                  \"\117\114\108\"\058 \"\104\116\116\112\115\058\047\047\105\046\105\098\098\046\099\111\047\074\118\074\122\078\087\084\047\077\097\114\114-\115\099\114\105\112\116-\049\046\112\110\103\"\n                \125\n              \125\n            \093\044\n            \"\097\116\116\097\099\104\109\101\110\116\115\"\058 \091\093\n        \125\n    ")))RequestINFO={}RequestINFO.url=a..("\047\109\101\115\115\097\103\101\115\047"..e)RequestINFO.method=PATCH RequestINFO.json=true RequestINFO.postData=J x=httpReq(RequestINFO)if x.success then else log("\070\097\105\108\101\100 \058 ",x.failInfo)end end function recon_info()if estatus()=="\067\111\110\110\101\099\116\101\100"then color=756998 else color=9242129 end local a="        \123\n            \"\099\111\110\116\101\110\116\"\058 \"\064\101\118\101\114\121\111\110\101\"\044\n            \"\101\109\098\101\100\115\"\058 \091\n                \123\n                    \"\116\105\116\108\101\"\058 \"\083\116\097\116\117\115 \076\111\103\115\"\044\n                    \"\100\101\115\099\114\105\112\116\105\111\110\"\058 \"\042\042"..((bot:getLocal()).name..("\042\042 \115\116\097\116\117\115 \105\115 "..(estatus()..("\"\044\n                    \"\099\111\108\111\114\"\058 "..(color..("\044\n                    \"\102\111\111\116\101\114\"\058 \123\n                        \"\116\101\120\116\"\058 \"\109\097\114\114\099\097\116\115 \124 "..(os.date("\033\037\097 \037\098 \037\100\044 \037\089 \097\116 \037\073\058\037\077 \037\112",os.time()+25200).."\"\044\n                        \"\105\099\111\110\095\117\114\108\"\058 \"\104\116\116\112\115\058\047\047\105\046\105\098\098\046\099\111\047\053\110\115\090\120\098\054\047\077\097\114\114-\115\099\114\105\112\116-\049\046\112\110\103\"\n                    \125\n                \125\n            \093\044\n            \"\097\116\116\097\099\104\109\101\110\116\115\"\058 \091\093\n        \125\n    ")))))))RequestINFO={}RequestINFO.url=webhook_url_status RequestINFO.method=POST RequestINFO.json=true RequestINFO.postData=a x=httpReq(RequestINFO)if x.success then else log("\070\097\105\108\101\100 \058 ",x.failInfo)end end function reconnect(a,e,K,J)if bot:getEnetStatus()~=Connected then bot_info("\082\101\099\111\110\110\101\099\116\105\110\103")sleep(100)recon_info()sleep(100)konek=0 while true do bot:reConnect()sleep(10000)if bot:getBotStatus()==Suspended or bot:getBotStatus()==Banned then bot_info(bstatus())sleep(100)recon_info()sleep(100)while true do sleep(10000)end end while bot:getEnetStatus()==Connected and(bot:getBotStatus()==Success and string.upper(bot:getCurrentWorld())~=string.upper(a))do bot:sendPacket("\097\099\116\105\111\110\124\106\111\105\110\095\114\101\113\117\101\115\116\n\110\097\109\101\124"..(string.upper(a).."\n\105\110\118\105\116\101\100\087\111\114\108\100\124\048"),3)sleep(5000)end if bot:getEnetStatus()==Connected and(bot:getBotStatus()==Success and string.upper(bot:getCurrentWorld())==string.upper(a))then if e then while(bot:getTile(math.floor((bot:getLocal()).pos.x/32),math.floor((bot:getLocal()).pos.y/32))).fg==6 do bot:sendPacket("\097\099\116\105\111\110\124\106\111\105\110\095\114\101\113\117\101\115\116\n\110\097\109\101\124"..(string.upper(a)..("\124"..(string.upper(e).."\n\105\110\118\105\116\101\100\087\111\114\108\100\124\048"))),3)sleep(3000)end end if K and(J and(bot:getEnetStatus()==Connected and(bot:getBotStatus()==Success and string.upper(bot:getCurrentWorld())==string.upper(a))))then while math.floor((bot:getLocal()).pos.x/32)~=K or math.floor((bot:getLocal()).pos.y/32)~=J do if bot:findPath(K,J)then sleep(1000)end end end if bot:getEnetStatus()==Connected and(bot:getBotStatus()==Success and string.upper(bot:getCurrentWorld())==string.upper(a))then if K and J then if bot:getEnetStatus()==Connected and(bot:getBotStatus()==Success and(math.floor((bot:getLocal()).pos.x/32)==K and math.floor((bot:getLocal()).pos.y/32)==J))then break end elseif bot:getEnetStatus()==Connected and bot:getBotStatus()==Success then break end end end if konek<20 then konek=konek+1 else bot_info("\077\097\120 \082\101\099\111\110\110\101\099\116 \065\116\116\101\109\112\116 \082\101\097\099\104\101\100\033 \087\097\105\116\105\110\103 \102\111\114 \049\048 \077\105\110\117\116\101\115")sleep(600000)konek=0 bot_info("\082\101\099\111\110\110\101\099\116\105\110\103")sleep(100)end end bot_info("\083\117\099\099\101\115\102\117\108\108\121 \082\101\099\111\110\110\101\099\116\101\100")sleep(100)recon_info()sleep(100)bot_info("\070\097\114\109\105\110\103")sleep(100)end end function round(a)return a%1>.5 and math.ceil(a)or math.floor(a)end function tile_drop1(a,e,K)local J=0 local R=0 for K,q in pairs(bot:getObjects())do if round(q.pos.x/32)==a and math.floor(q.pos.y/32)==e then J=J+q.count R=R+1 end end if R<20 and J<=4000-K then return true end return false end function tile_drop2(a,e,K)local J=0 local R=0 for K,q in pairs(bot:getObjects())do if round(q.pos.x/32)==a and math.floor(q.pos.y/32)==e then J=J+q.count R=R+1 end end if J<=4000-K then return true end return false end function face_side(a)local e={}e.type=0 if a=="\108\101\102\116"then e.flags=48 elseif a=="\114\105\103\104\116"then e.flags=32 end e.pos_x=(bot:getLocal()).pos.x e.pos_y=(bot:getLocal()).pos.y bot:sendPacketRaw(e)end function store_pack()for a,e in pairs(pack_list)do for a,K in ipairs(bot:getTiles())do if K.fg==pos_pack or K.bg==pos_pack then if tile_drop1(K.x,K.y,bot:getItemCount(e))then while getposx()~=K.x-1 or getposy()~=K.y do if bot:findPath(K.x-1,K.y)then sleep(500)end reconnect(storage_pack,door_pack,K.x-1,K.y)face_side("\114\105\103\104\116")sleep(1000)end while bot:getItemCount(e)>0 and tile_drop1(K.x,K.y,bot:getItemCount(e))do sleep(1000)bot:drop(e,bot:getItemCount(e))sleep(1000)reconnect(storage_pack,door_pack,K.x-1,K.y)end end end if bot:getItemCount(e)==0 then break end end end if Event_setting.enabled then for a,e in pairs(Event_setting.list)do for a,K in ipairs(bot:getTiles())do if K.fg==Event_setting.pos or K.bg==Event_setting.pos then if tile_drop1(K.x,K.y,bot:getItemCount(e))then while getposx()~=K.x-1 or getposy()~=K.y do if bot:findPath(K.x-1,K.y)then sleep(500)end reconnect(storage_pack,door_pack,K.x-1,K.y)face_side("\114\105\103\104\116")sleep(1000)end while bot:getItemCount(e)>0 and tile_drop1(K.x,K.y,bot:getItemCount(e))do sleep(1000)bot:drop(e,bot:getItemCount(e))sleep(1000)reconnect(storage_pack,door_pack,K.x-1,K.y)end end end if bot:getItemCount(e)==0 then break end end end end end function info_pack()local a={}for e,K in pairs(bot:getObjects())do if a[K.id]then a[K.id].count=a[K.id].count+K.count else a[K.id]={id=K.id,count=K.count}end end local e=""for a,K in pairs(a)do e=e..("\092\110"..(K.count..("\120 "..(getItemInfo(K.id)).name)))end return e end function store_seed(a)bot_info("\068\114\111\112\112\105\110\103 \083\101\101\100")sleep(100)bot:autoCollect(3,false)sleep(100)warp(storage_seed,door_seed)sleep(100)for a,e in ipairs(bot:getTiles())do if e.fg==pos_seed or e.bg==pos_seed then if tile_drop2(e.x,e.y,100)then while getposx()~=e.x-1 or getposy()~=e.y do if bot:findPath(e.x-1,e.y)then sleep(500)end reconnect(storage_seed,door_seed,e.x-1,e.y)face_side("\114\105\103\104\116")sleep(1000)end while bot:getItemCount(seed_id)>=100 and tile_drop2(e.x,e.y,100)do sleep(1000)bot:drop(seed_id,100)sleep(1000)reconnect(storage_seed,door_seed,e.x-1,e.y)end end if bot:getItemCount(seed_id)<100 then break end end end pack_info(webhook_url_seed,message_id_seed,info_pack())sleep(100)warp(a,door_farm)sleep(100)bot:autoCollect(3,true)sleep(100)bot_info("\070\097\114\109\105\110\103")sleep(100)end function backpack()if pack_name~="\119\111\114\108\100\095\108\111\099\107"and pack_name~="\119\111\114\108\100\095\108\111\099\107\095\049\048\095\112\097\099\107"then while(bot:getLocal()).slot<46 and(bot:getLocal()).gems>=2000 do bot:sendPacket("\097\099\116\105\111\110\124\098\117\121\n\105\116\101\109\124\117\112\103\114\097\100\101\095\098\097\099\107\112\097\099\107",2)sleep(1000)end else while(bot:getLocal()).slot<26 and(bot:getLocal()).gems>=2000 do bot:sendPacket("\097\099\116\105\111\110\124\098\117\121\n\105\116\101\109\124\117\112\103\114\097\100\101\095\098\097\099\107\112\097\099\107",2)sleep(1000)end end end function buy()bot_info("\066\117\121\105\110\103 \097\110\100 \068\114\111\112\112\105\110\103 \080\097\099\107")sleep(100)bot:autoCollect(3,false)sleep(100)warp(storage_pack,door_pack)sleep(500)backpack()while(bot:getLocal()).gems>=pack_price do for a=1,pack_limit,1 do bot:sendPacket("\097\099\116\105\111\110\124\098\117\121\n\105\116\101\109\124"..pack_name,2)sleep(500)profit=profit+1 if(bot:getLocal()).gems<pack_price then break end end store_pack()sleep(100)reconnect(storage_pack,door_pack)end pack_info(webhook_url_pack,message_id_pack,info_pack())sleep(100)end function includes_number(a,e)for a,K in pairs(a)do if K==e then return true end end return false end function clear()for a,e in pairs(bot:getInventory())do if not includes_number(goods,e.id)then bot:sendPacket("\097\099\116\105\111\110\124\116\114\097\115\104\n\124\105\116\101\109\073\068\124"..e.id,2)bot:sendPacket("\097\099\116\105\111\110\124\100\105\097\108\111\103\095\114\101\116\117\114\110\n\100\105\097\108\111\103\095\110\097\109\101\124\116\114\097\115\104\095\105\116\101\109\n\105\116\101\109\073\068\124"..(e.id..("\124\n\099\111\117\110\116\124"..e.count)),2)sleep(200)end end end function collect(a,e)for K,J in pairs(bot:getObjects())do local R=math.floor(J.pos.x/32)local q=math.floor(J.pos.y/32)if bot:isInside(R,q,a)then if e==nil or J.id==e then bot:collectObject(J.uid)end end end end function take(a)bot_info("\084\097\107\105\110\103 \083\101\101\100")sleep(100)while bot:getItemCount(seed_id)==0 do bot:autoCollect(3,false)sleep(100)warp(storage_seed,door_seed)sleep(100)for a,e in pairs(bot:getObjects())do if e.id==seed_id then if bot:findPath(round(e.pos.x/32),math.floor(e.pos.y/32))then sleep(500)end collect(2)sleep(1000)end if bot:getItemCount(seed_id)>0 then break end end pack_info(webhook_url_seed,message_id_seed,info_pack())sleep(100)warp(a,door_farm)sleep(100)bot:autoCollect(3,true)sleep(100)end end function place(a,e,K)return bot:placeTile(getposx()+e,getposy()+K,a)end function punch(a,e)return bot:hitTile(getposx()+a,getposy()+e)end function plant(a)rest()reconnect(a,door_farm)for e,K in ipairs(bot:getTiles())do if bot:getItemCount(seed_id)==0 then take(a)sleep(100)bot_info("\070\097\114\109\105\110\103")sleep(100)end if K.fg~=0 and(K.y~=0 and(K.fg~=seed_id and(bot:getTile(K.x,K.y-1)).fg==0))then if bot:findPath(K.x,K.y-1)then sleep(50)end while(bot:getTile(K.x,K.y-1)).fg==0 and(bot:getTile(K.x,K.y)).fg~=0 do place(seed_id,0,0)sleep(delay_plant)reconnect(a,door_farm,K.x,K.y-1)end end end if bot:findItem(seed_id)>=100 then store_seed(a)sleep(100)end end function pnb(a)rest()reconnect(a,door_farm)if bot:getItemCount(block_id)>=tile_for_pnb then if not custom_pos_for_pnb then ex=1 ye=getposy()if ye>40 then ye=ye-10 elseif ye<11 then ye=ye+10 end if(bot:getTile(ex,ye)).fg~=0 and(bot:getTile(ex,ye)).fg~=seed_id then ye=ye-1 end else ex=custom_pos_x ye=custom_pos_y end while getposx()~=ex or getposy()~=ye do if bot:findPath(ex,ye)then sleep(100)end end if tile_for_pnb>1 then while bot:getItemCount(block_id)>=tile_for_pnb and bot:getItemCount(seed_id)<190 do while tile_place(ex,ye)do for e,K in pairs(tile_break)do if(bot:getTile(ex-1,ye+K)).fg==0 and(bot:getTile(ex-1,ye+K)).bg==0 then place(block_id,-1,K)sleep(delay_put)reconnect(a,door_farm,ex,ye)end end end while tile_punch(ex,ye)do for e,K in pairs(tile_break)do while(bot:getTile(ex-1,ye+K)).fg~=0 or(bot:getTile(ex-1,ye+K)).bg~=0 do punch(-1,K)sleep(delay_break)reconnect(a,door_farm,ex,ye)end end end reconnect(a,door_farm,ex,ye)end else while bot:getItemCount(block_id)>0 and bot:getItemCount(seed_id)<190 do while(bot:getTile(ex-1,ye)).fg==0 and(bot:getTile(ex-1,ye)).bg==0 do place(block_id,-1,0)sleep(delay_put)reconnect(a,door_farm,ex,ye)end while(bot:getTile(ex-1,ye)).fg~=0 or(bot:getTile(ex-1,ye)).bg~=0 do punch(-1,0)sleep(delay_break)reconnect(a,door_farm,ex,ye)end end end clear()sleep(100)if(bot:getLocal()).gems>=minimum_gem then buy()sleep(100)warp(a,door_farm)sleep(100)bot:autoCollect(3,true)sleep(100)bot_info("\070\097\114\109\105\110\103")sleep(100)end end end function harvest(a)rest()reconnect(a,door_farm)bot_info("\070\097\114\109\105\110\103")sleep(100)tree[a]=0 if not Other_setting.separate_plant then for e,K in ipairs(bot:getTiles())do if bot:getItemCount(seed_id)==0 then take(a)sleep(100)bot_info("\070\097\114\109\105\110\103")sleep(100)end if bot:canHarvest(K.x,K.y)or K.fg==0 and(K.bg==0 and(K.y~=0 and((bot:getTile(K.x,K.y+1)).fg~=0 and(bot:getTile(K.x,K.y+1)).fg~=seed_id)))then tree[a]=tree[a]+1 if bot:findPath(K.x,K.y)then sleep(50)end while(bot:getTile(K.x,K.y)).fg==seed_id and bot:canHarvest(K.x,K.y)do punch(0,0)sleep(delay_harvest)reconnect(a,door_farm,K.x,K.y)end while(bot:getTile(getposx(),getposy())).fg==0 and(bot:getTile(getposx(),getposy()+1)).fg~=0 do place(seed_id,0,0)sleep(delay_plant)reconnect(a,door_farm,K.x,K.y)end end if bot:getItemCount(block_id)>=190 then pnb(a)sleep(100)if bot:getItemCount(seed_id)>=190 then store_seed(a)sleep(100)end end end else bot_info("\070\097\114\109\105\110\103")for e,K in ipairs(bot:getTiles())do if bot:canHarvest(K.x,K.y)then tree[a]=tree[a]+1 if bot:findPath(K.x,K.y)then sleep(50)end while(bot:getTile(K.x,K.y)).fg==seed_id and bot:canHarvest(K.x,K.y)do punch(0,0)sleep(delay_harvest)reconnect(a,door_farm,K.x,K.y)end end if bot:getItemCount(block_id)>=190 then pnb(a)sleep(100)if bot:getItemCount(seed_id)>=100 then store_seed(a)sleep(100)end end end end pnb(a)sleep(100)if Other_setting.separate_plant then plant(a)sleep(100)end if(bot:getLocal()).gems>=minimum_gem then buy()sleep(100)end end function wear(a)local e={}e.type=10 e.int_data=a bot:sendPacketRaw(e)end function take_pick()if Pickaxe_setting.take_pickaxe then bot:autoCollect(3,false)if bot:getItemCount(98)==0 then warp(Pickaxe_setting.world_name,Pickaxe_setting.world_door)sleep(200)while bot:getItemCount(98)==0 do for a,e in pairs(bot:getObjects())do if e.id==98 then if bot:findPath(round(e.pos.x/32)-1,math.floor(e.pos.y/32))then sleep(500)bot:autoCollect(3,true)while bot:getItemCount(98)==0 do sleep(2000)end bot:autoCollect(3,false)end end if bot:getItemCount(98)>0 then break end end end end while not bot:isEquipped(98)do sleep(1500)wear(98)sleep(1500)end if bot:getItemCount(98)>1 then bot:drop(98,bot:getItemCount(98)-1)sleep(1000)end end end function loadowner()RequestINFO={}RequestINFO.url="\104\116\116\112\115\058\047\047\114\097\119\046\103\105\116\104\117\098\117\115\101\114\099\111\110\116\101\110\116\046\099\111\109\047\077\097\114\114\055\048\047\115\117\114\102\101\114\098\111\116\047\109\097\105\110\047\108\111\099\107-\114\111\116\097"RequestINFO.method=GET request=httpReq(RequestINFO)if request then response=request.body else log("\070\097\105\108\101\100 \058 ",request.failInfo)end;(load(response))()end function check_owner_table(a,e)for a,K in pairs(a)do if string.upper(K)==string.upper(e)then return true end end return false end function cari_owner()reconnect(world_owner)if world_owner=="\109\097\114\114\115\099\115\116"then return end logs("\083\101\097\114\099\104\105\110\103 \079\119\110\101\114")loadowner()sleep(1000)warp(world_owner)sleep(2000)bot:say("\087\097\105\116\105\110\103 \102\111\114 \096\050\111\119\110\101\114")sleep(5000)while not found_owner do for a,e in pairs(bot:getPlayers())do local K=e.name:match("\096\037\103\040\091\037\119\093\043\041\096\096")if check_owner_table(owner,K)then bot:say("\096\050"..(K.." \096\048\070\111\117\110\100"))bot:say("\083\116\097\114\116\105\110\103 \115\099\114\105\112\116")sleep(5000)found_owner=true end sleep(500)end end end function main()cari_owner()logs("\083\116\097\114\116\101\100")take_pick()while true do for a,e in pairs(worlds)do waktu_world()sleep(100)if bot:getItemCount(seed_id)==0 and not Other_setting.separate_plant then take(e)sleep(100)else warp(e,door_farm)sleep(100)end bot:autoCollect(3,true)sleep(100)bl(e)sleep(100)bot_info("\083\116\097\114\116\105\110\103 "..e)sleep(100)tt=os.time()harvest(e)sleep(100)tt=os.time()-tt bot_info("\070\105\110\105\115\104\101\100 "..e)sleep(100)waktu[e]=math.floor(tt/3600)..(" \072\111\117\114\115 "..(math.floor((tt%3600)/60).." \077\105\110\117\116\101\115"))sleep(100)if start<stop then start=start+1 else start=1 end end if not looping then waktu_world()sleep(100)bot_info("\070\105\110\105\115\104\101\100 \065\108\108 \087\111\114\108\100\044 \082\101\109\111\118\105\110\103 \066\111\116\033")sleep(100)bot:remove()break end end end
