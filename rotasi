bot=getBot()worlds={}fossil={}tileBreak={}tree={}treeun={}profit=0 t=os.time()countSeed=0 blacklistTile=false worldList=Bot[(bot:getLocal()).name:upper()].farmList start=Bot[(bot:getLocal()).name:upper()].startFrom stop=#Bot[(bot:getLocal()).name:upper()].farmList doorFarm=Bot[(bot:getLocal()).name:upper()].doorFarm tileNumber=Bot[(bot:getLocal()).name:upper()].tileNumber customPNB=Bot[(bot:getLocal()).name:upper()].customPNB customX=Bot[(bot:getLocal()).name:upper()].customX customY=Bot[(bot:getLocal()).name:upper()].customY autoPickaxe=Bot[(bot:getLocal()).name:upper()].autoPickaxe pickStorage=Bot[(bot:getLocal()).name:upper()].pickStorage doorPick=Bot[(bot:getLocal()).name:upper()].doorPick for V=start,stop,1 do table.insert(worlds,worldList[V])end if looping then for V=0,start-1,1 do table.insert(worlds,worldList[V])end end for V,t in pairs(packList)do table.insert(goods,t)end for V=math.floor(tileNumber/2),1,-1 do V=V*-1 table.insert(tileBreak,V)end for V=0,math.ceil(tileNumber/2)-1,1 do table.insert(tileBreak,V)end function bl(V)blist={}fossil[V:upper()]=0 for t,l in ipairs(bot:getTiles({skip=true}))do if l.fg==6 then doorX=l.x doorY=l.y elseif l.fg==3918 then fossil[V:upper()]=fossil[V:upper()]+1 end end if blacklistTile then for V,t in pairs(blacklist)do table.insert(blist,{x=doorX+t.x,y=doorY+t.y})end end end function tilePunch(V,t)for l,U in pairs(tileBreak)do if(bot:getTile(V-1,t+U)).fg~=0 or(bot:getTile(V-1,t+U)).bg~=0 then return true end end return false end function tilePlace(V,t)for l,U in pairs(tileBreak)do if(bot:getTile(V-1,t+U)).fg==0 and(bot:getTile(V-1,t+U)).bg==0 then return true end end return false end function warp(V,t)while(bot:getWorld()).name:upper()~=V:upper()do bot:sendPacket("\097\099\116\105\111\110\124\106\111\105\110\095\114\101\113\117\101\115\116\n\110\097\109\101\124"..(V:upper().."\n\105\110\118\105\116\101\100\087\111\114\108\100\124\048"),3)sleep(delayJoinWorld)reconnect(V,t)end if t~=""and t~=nil then while(bot:getTile(math.floor((bot:getLocal()).pos.x/32),math.floor((bot:getLocal()).pos.y/32))).fg==6 do bot:sendPacket("\097\099\116\105\111\110\124\106\111\105\110\095\114\101\113\117\101\115\116\n\110\097\109\101\124"..(V:upper()..("\124"..(t:upper().."\n\105\110\118\105\116\101\100\087\111\114\108\100\124\048"))),3)sleep(delayJoinWorld)reconnect(V,t)end end end function countWorld(V)tree[V:upper()]=0 treeun[V:upper()]=0 if bot:inWorld()then for t,l in ipairs(bot:getTiles({skip=true}))do if l.fg==seedId and bot:hasAccessToTile(l.x,l.y)then if bot:canHarvest(l.x,l.y)then tree[V:upper()]=tree[V:upper()]+1 else treeun[V:upper()]=treeun[V:upper()]+1 end end end end end function botInfo(V)te=os.time()-t fossill=fossil[(bot:getWorld()).name:upper()]or 0 local l="    \123\n        \"\099\111\110\116\101\110\116\"\058 \110\117\108\108\044\n        \"\101\109\098\101\100\115\"\058 \091\n        \123\n            \"\116\105\116\108\101\"\058 \"\082\111\116\097\116\105\111\110 \083\099\114\105\112\116 \066\121 \079\109\110\105\115\099\105\117\115\"\044\n            \"\099\111\108\111\114\"\058 \049\054\051\055\055\048\050\048\044\n            \"\102\105\101\108\100\115\"\058 \091\n            \123\n                \"\110\097\109\101\"\058 \"\084\065\083\075 \073\078\070\079\082\077\065\084\073\079\078\"\044\n                \"\118\097\108\117\101\"\058 \"\060\058\100\117\098\115\116\101\112\058\049\050\053\054\049\050\049\052\055\055\050\052\050\050\057\048\050\052\055\062 \124 \067\117\114\114\101\110\116 \084\097\115\107 \058 "..(V..("\092\110\060\058\099\114\121\115\116\097\108\099\108\111\099\107\058\049\050\053\054\049\050\048\051\056\054\048\056\049\051\057\048\054\048\052\062 \124 \085\112\116\105\109\101 \058 "..(math.floor(te/86400)..(" \068\097\121\115 "..(math.floor((te%86400)/3600)..(" \072\111\117\114\115 "..(math.floor(((te%86400)%3600)/60)..(" \077\105\110\117\116\101\115\"\n            \125\044\n            \123\n                \"\110\097\109\101\"\058 \"\066\079\084 \073\078\070