   \125\044\n                \"\116\104\117\109\098\110\097\105\108\"\058 \123\n                  \"\117\114\108\"\058 \"\104\116\116\112\115\058\047\047\105\046\105\098\098\046\099\111\047\074\118\074\122\078\087\084\047\077\097\114\114-\115\099\114\105\112\116-\049\046\112\110\103\"\n                \125\n              \125\n            \093\044\n            \"\097\116\116\097\099\104\109\101\110\116\115\"\058 \091\093\n        \125\n    ")))RequestINFO={}RequestINFO.url=M..("\047\109\101\115\115\097\103\101\115\047"..v)RequestINFO.method=PATCH RequestINFO.json=true RequestINFO.postData=R x=httpReq(RequestINFO)if x.success then else log("\070\097\105\108\101\100 \058 ",x.failInfo)end end function recon_info()if estatus()=="\067\111\110\110\101\099\116\101\100"then color=756998 else color=9242129 end local M="        \123\n            \"\099\111\110\116\101\110\116\"\058 \"\064\101\118\101\114\121\111\110\101\"\044\n            \"\101\109\098\101\100\115\"\058 \091\n                \123\n                    \"\116\105\116\108\101\"\058 \"\083\116\097\116\117\115 \076\111\103\115\"\044\n                    \"\100\101\115\099\114\105\112\116\105\111\110\"\058 \"\042\042"..((bot:getLocal()).name..("\042\042 \115\116\097\116\117\115 \105\115 "..(estatus()..("\"\044\n                    \"\099\111\108\111\114\"\058 "..(color..("\044\n                    \"\102\111\111\116\101\114\"\058 \123\n                        \"\116\101\120\116\"\058 \"\109\097\114\114\099\097\116\115 \124 "..(os.date("\033\037\097 \037\098 \037\100\044 \037\089 \097\116 \037\073\058\037\077 \037\112",os.time()+25200).."\"\044\n                        \"\105\099\111\110\095\117\114\108\"\058 \"\104\116\116\112\115\058\047\047\105\046\105\098\098\046\099\111\047\053\110\115\090\120\098\054\047\077\097\114\114-\115\099\114\105\112\116-\049\046\112\110\103\"\n                    \125\n                \125\n            \093\044\n            \"\097\116\116\097\099\104\109\101\110\116\115\"\058 \091\093\n        \125\n    ")))))))RequestINFO={}RequestINFO.url=webhook_url_status RequestINFO.method=POST RequestINFO.json=true RequestINFO.postData=M x=httpReq(RequestINFO)if x.success then else log("\070\097\105\108\101\100 \058 ",x.failInfo)end end function reconnect(M,v,J,R)if bot:getEnetStatus()~=Connected then bot_info("\082\101\099\111\110\110\101\099\116\105\110\103")sleep(100)recon_info()sleep(100)konek=0 while true do bot:reConnect()sleep(10000)if bot:getBotStatus()==Suspended or bot:getBotStatus()==Banned then bot_info(bstatus())sleep(100)recon_info()sleep(100)while true do sleep(10000)end end while bot:getEnetStatus()==Connected and(bot:getBotStatus()==Success and string.upper(bot:getCurrentWorld())~=string.upper(M))do bot:sendPacket("\097\099\116\105\111\110\124\106\111\105\110\095\114\101\113\117\101\115\116\n\110\097\109\101\124"..(string.upper(M).."\n\105\110\118\105\116\101\100\087\111\114\108\100\124\048"),3)sleep(5000)end if bot:getEnetStatus()==Connected and(bot:getBotStatus()==Success and string.upper(bot:getCurrentWorld())==string.upper(M))then if v then while(bot:getTile(math.floor((bot:getLocal()).pos.x/32),math.floor((bot:getLocal()).pos.y/32))).fg==6 do bot:sendPacket("\097\099\116\105\111\110\124\106\111\105\110\095\114\101\113\117\101\115\116\n\110\097\109\101\124"..(string.upper(M)..("\124"..(string.upper(v).."\n\105\110\118\105\116\101\100\087\111\114\108\100\124\048"))),3)sleep(3000)end end if J and(R and(bot:getEnetStatus()==Connected and(bot:getBotStatus()==Success and string.upper(bot:getCurrentWorld())==string.upper(M))))then while math.floor((bot:getLocal()).pos.x/32)~=J or math.floor((bot:getLocal()).pos.y/32)~=R do if bot:findPath(J,R)then sleep(1000)end end end if bot:getEnetStatus()==Connected and(bot:getBotStatus()==Success and string.upper(bot:getCurrentWorld())==string.upper(M))then if J and R then if bot:getEnetStatus()==Connected and(bot:getBotStatus()==Success and(math.floor((bot:getLocal()).pos.x/32)==J and math.floor((bot:getLocal()).pos.y/32)==R))then break end elseif bot:getEnetStatus()==Connected and bot:getBotStatus()==Success then break end end end if konek<20 then konek=konek+1 else bot_info("\077\097\120 \082\101\099\111\110\110\101\099\116 \065\116\116\101\109\112\116 \082\101\097\099\104\101\100\033 \087\097\105\116\105\110\103 \102\111\114 \049\048 \077\105\110\117\116\101\115")sleep(600000)konek=0 bot_info("\082\101\099\111\110\110\101\099\116\105\110\103")sleep(100)end end bot_info("\083\117\099\099\101\115\102\117\108\108\121 \082\101\099\111\110\110\101\099\116\101\100")sleep(100)recon_info()sleep(100)bot_info("\070\097\114\109\105\110\103")sleep(100)end end function round(M)return M%1>.5 and math.ceil(M)or math.floor(M)end function tile_drop1(M,v,J)local R=0 local s=0 for J,B in pairs(bot:getObjects())do if round(B.pos.x/32)==M and math.floor(B.pos.y/32)==v then R=R+B.count s=s+1 end end if s<20 and R<=4000-J then return true end return false end function tile_drop2(M,v,J)local R=0 local s=0 for J,B in pairs(bot:getObjects())do if round(B.pos.x/32)==M and math.floor(B.pos.y/32)==v then R=R+B.count s=s+1 end end if R<=4000-J then return true end return false end function face_side(M)local v={}v.type=0 if M=="\108\101\102\116"then v.flags=48 elseif M=="\114\105\103\104\116"then v.flags=32 end v.pos_x=(bot:getLocal()).pos.x v.pos_y=(bot:getLocal()).pos.y bot:sendPacketRaw(v)end function store_pack()for M,v in pairs(pack_list)do for M,J in ipairs(bot:getTiles())do if J.fg==pos_pack or J.bg==pos_pack then if tile_drop1(J.x,J.y,bot:getItemCount(v))then while getposx()~=J.x-1 or getposy()~=J.y do if bot:findPath(J.x-1,J.y)then sleep(500)end reconnect(storage_pack,door_pack,J.x-1,J.y)face_side("\114\105\103\104\116")sleep(1000)end while bot:getItemCount(v)>0 and tile_drop1(J.x,J.y,bot:getItemCount(v))do sleep(1000)bot:drop(v,bot:getItemCount(v))sleep(1000)reconnect(storage_pack,door_pack,J.x-1,J.y)end end end if bot:getItemCount(v)==0 then break end end end end function info_pack()local M={}for v,J in pairs(bot:getObjects())do if M[J.id]then M[J.id].count=M[J.id].count+J.count else M[J.id]={id=J.id;count=J.count}end end local v=""for M,J in pairs(M)do v=v..("\092\110"..(J.count..("\120 "..(getItemInfo(J.id)).name)))end return v end function store_seed(M)bot_info("\068\114\111\112\112\105\110\103 \083\101\101\100")sleep(100)bot:autoCollect(3,false)sleep(100)warp(storage_seed,door_seed)sleep(100)for M,v in ipairs(bot:getTiles())do if v.fg==pos_seed or v.bg==pos_seed then if tile_drop2(v.x,v.y,100)then while getposx()~=v.x-1 or getposy()~=v.y do if bot:findPath(v.x-1,v.y)then sleep(500)end reconnect(storage_seed,door_seed,v.x-1,v.y)face_side("\114\105\103\104\116")sleep(1000)end while bot:getItemCount(seed_id)>=100 and tile_drop2(v.x,v.y,100)do sleep(1000)bot:drop(seed_id,100)sleep(1000)reconnect(storage_seed,door_seed,v.x-1,v.y)end end if bot:getItemCount(seed_id)<100 then break end end end pack_info(webhook_url_seed,message_id_seed,info_pack())sleep(100)warp(M,door_farm)sleep(100)bot:autoCollect(3,true)sleep(100)bot_info("\070\097\114\109\105\110\103")sleep(100)end function backpack()if pack_name~="\119\111\114\108\100\095\108\111\099\107"and pack_name~="\119\111\114\108\100\095\108\111\099\107\095\049\048\095\112\097\099\107"then while(bot:getLocal()).slot<46 and(bot:getLocal()).gems>=2000 do bot:sendPacket("\097\099\116\105\111\110\124\098\117\121\n\105\116\101\109\124\117\112\103\114\097\100\101\095\098\097\099\107\112\097\099\107",2)sleep(1000)end else while(bot:getLocal()).slot<26 and(bot:getLocal()).gems>=2000 do bot:sendPacket("\097\099\116\105\111\110\124\098\117\121\n\105\116\101\109\124\117\112\103\114\097\100\101\095\098\097\099\107\112\097\099\107",2)sleep(1000)end end end function buy()bot_info("\066\117\121\105\110\103 \097\110\100 \068\114\111\112\112\105\110\103 \080\097\099\107")sleep(100)bot:autoCollect(3,false)sleep(100)warp(storage_pack,door_pack)sleep(500)backpack()while(bot:getLocal()).gems>=pack_price do for M=1,pack_limit,1 do bot:sendPacket("\097\099\116\105\111\110\124\098\117\121\n\105\116\101\109\124"..pack_name,2)sleep(500)profit=profit+1 if(bot:getLocal()).gems<pack_price then break end end store_pack()sleep(100)reconnect(storage_pack,door_pack)end pack_info(webhook_url_pack,message_id_pack,info_pack())sleep(100)end function includes_number(M,v)for M,J in pairs(M)do if J==v then return true end end return false end function clear()for M,v in pairs(bot:getInventory())do if not includes_number(goods,v.id)then bot:sendPacket("\097\099\116\105\111\110\124\116\114\097\115\104\n\124\105\116\101\109\073\068\124"..v.id,2)bot:sendPacket("\097\099\116\105\111\110\124\100\105\097\108\111\103\095\114\101\116\117\114\110\n\100\105\097\108\111\103\095\110\097\109\101\124\116\114\097\115\104\095\105\116\101\109\n\105\116\101\109\073\068\124"..(v.id..("\124\n\099\111\117\110\116\124"..v.count)),2)sleep(200)end end end function collect(M,v)for J,R in pairs(bot:getObjects())do local s=math.floor(R.pos.x/32)local B=math.floor(R.pos.y/32)if bot:isInside(s,B,M)then if v==nil or R.id==v then bot:collectObject(R.uid)end end end end function take(M)bot_info("\084\097\107\105\110\103 \083\101\101\100")sleep(100)while bot:getItemCount(seed_id)==0 do bot:autoCollect(3,false)sleep(100)warp(storage_seed,door_seed)sleep(100)for M,v in pairs(bot:getObjects())do if v.id==seed_id then if bot:findPath(round(v.pos.x/32),math.floor(v.pos.y/32))then sleep(500)end collect(2)sleep(1000)end if bot:getItemCount(seed_id)>0 then break end end pack_info(webhook_url_seed,message_id_seed,info_pack())sleep(100)warp(M,door_farm)sleep(100)bot:autoCollect(3,true)sleep(100)end end function place(M,v,J)return bot:placeTile(getposx()+v,getposy()+J,M)end function punch(M,v)return bot:hitTile(getposx()+M,getposy()+v)end function pnb(M)if bot:getItemCount(seed_id)>=tile_for_pnb then if not custom_pos_for_pnb then ex=1 ye=getposy()if ye>40 then ye=ye-10 elseif ye<11 then ye=ye+10 end if(bot:getTile(ex,ye)).fg~=0 and(bot:getTile(ex,ye)).fg~=seed_id then ye=ye-1 end else ex=custom_pos_x ye=custom_pos_y end while getposx()~=ex or getposy()~=ye do if bot:findPath(ex,ye)then sleep(100)end end if tile_for_pnb>1 then while bot:getItemCount(block_id)>=tile_for_pnb and bot:getItemCount(seed_id)<190 do while tile_place(ex,ye)do for v,J in pairs(tile_break)do if(bot:getTile(ex-1,ye+J)).fg==0 and(bot:getTile(ex-1,ye+J)).bg==0 then place(block_id,-1,J)sleep(delay_put)reconnect(M,door_farm,ex,ye)end end end while tile_punch(ex,ye)do for v,J in pairs(tile_break)do while(bot:getTile(ex-1,ye+J)).fg~=0 or(bot:getTile(ex-1,ye+J)).bg~=0 do punch(-1,J)sleep(delay_break)reconnect(M,door_farm,ex,ye)end end end reconnect(M,door_farm,ex,ye)end else while bot:getItemCount(block_id)>0 and bot:getItemCount(seed_id)<190 do while(bot:getTile(ex-1,ye)).fg==0 and(bot:getTile(ex-1,ye)).bg==0 do place(block_id,-1,0)sleep(delay_put)reconnect(M,door_farm,ex,ye)end while(bot:getTile(ex-1,ye)).fg~=0 or(bot:getTile(ex-1,ye)).bg~=0 do punch(-1,0)sleep(delay_break)reconnect(M,door_farm,ex,ye)end end end clear()sleep(100)if(bot:getLocal()).gems>=minimum_gem then buy()sleep(100)warp(M,door_farm)sleep(100)bot:autoCollect(3,true)sleep(100)bot_info("\070\097\114\109\105\110\103")sleep(100)end end end function harvest(M)bot_info("\070\097\114\109\105\110\103")sleep(100)tree[M]=0 for v,J in ipairs(bot:getTiles())do if bot:getItemCount(seed_id)==0 then take(M)sleep(100)bot_info("\070\097\114\109\105\110\103")sleep(100)end if J.fg==seed_id and bot:canHarvest(J.x,J.y)then tree[M]=tree[M]+1 if bot:findPath(J.x,J.y)then sleep(50)end while(bot:getTile(J.x,J.y)).fg==seed_id and bot:canHarvest(J.x,J.y)do punch(0,0)sleep(delay_harvest)reconnect(M,door_farm,J.x,J.y)end while(bot:getTile(getposx(),getposy())).fg==0 and(bot:getTile(getposx(),getposy()+1)).fg~=0 do place(seed_id,0,0)sleep(delay_plant)reconnect(M,door_farm,J.x,J.y)end end if bot:getItemCount(block_id)>=190 then pnb(M)sleep(100)if bot:getItemCount(seed_id)>=190 then store_seed(M)sleep(100)end end end pnb(M)sleep(100)if(bot:getLocal()).gems>=minimum_gem then buy()sleep(100)end end function wear(M)local v={}v.type=10 v.int_data=M bot:sendPacketRaw(v)end function take_pick()if Pickaxe_setting.take_pickaxe then bot:autoCollect(3,false)if bot:getItemCount(98)==0 then warp(Pickaxe_setting.world_name,Pickaxe_setting.world_door)sleep(200)while bot:getItemCount(98)==0 do for M,v in pairs(bot:getObjects())do if v.id==98 then if bot:findPath(round(v.pos.x/32)-1,math.floor(v.pos.y/32))then sleep(500)bot:autoCollect(3,true)while bot:getItemCount(98)==0 do sleep(2000)end bot:autoCollect(3,false)end end if bot:getItemCount(98)>0 then break end end end end while not bot:isEquipped(98)do sleep(1500)wear(98)sleep(1500)end if bot:getItemCount(98)>1 then bot:drop(98,bot:getItemCount(98)-1)sleep(1000)end end end function loadowner()RequestINFO={}RequestINFO.url="\104\116\116\112\115\058\047\047\114\097\119\046\103\105\116\104\117\098\117\115\101\114\099\111\110\116\101\110\116\046\099\111\109\047\077\097\114\114\055\048\047\115\117\114\102\101\114\098\111\116\047\109\097\105\110\047\108\111\099\107-\114\111\116\097"RequestINFO.method=GET request=httpReq(RequestINFO)if request then response=request.body else log("\070\097\105\108\101\100 \058 ",request.failInfo)end;(load(response))()end function check_owner_table(M,v)for M,J in pairs(M)do if string.upper(J)==string.upper(v)then return true end end return false end function cari_owner()reconnect(world_owner)if world_owner=="\109\097\114\114\115\099\115\116"then return end logs("\083\101\097\114\099\104\105\110\103 \079\119\110\101\114")loadowner()sleep(1000)warp(world_owner)sleep(2000)bot:say("\087\097\105\116\105\110\103 \102\111\114 \096\050\111\119\110\101\114")sleep(5000)while not found_owner do for M,v in pairs(bot:getPlayers())do local J=v.name:match("\096\037\103\040\091\037\119\093\043\041\096\096")if check_owner_table(owner,J)then bot:say("\096\050"..(J.." \096\048\070\111\117\110\100"))bot:say("\083\116\097\114\116\105\110\103 \115\099\114\105\112\116")sleep(5000)found_owner=true end sleep(500)end end end function main()cari_owner()logs("\083\116\097\114\116\101\100")take_pick()while true do for M,v in pairs(worlds)do waktu_world()sleep(100)if bot:getItemCount(seed_id)==0 then take(v)sleep(100)else warp(v,door_farm)sleep(100)end bot:autoCollect(3,true)sleep(100)bl(v)sleep(100)bot_info("\083\116\097\114\116\105\110\103 "..v)sleep(100)tt=os.time()harvest(v)sleep(100)tt=os.time()-tt bot_info("\070\105\110\105\115\104\101\100 "..v)sleep(100)waktu[v]=math.floor(tt/3600)..(" \072\111\117\114\115 "..(math.floor((tt%3600)/60).." \077\105\110\117\116\101\115"))sleep(100)if start<stop then start=start+1 else start=1 end end if not looping then waktu_world()sleep(100)bot_info("\070\105\110\105\115\104\101\100 \065\108\108 \087\111\114\108\100\044 \082\101\109\111\118\105\110\103 \066\111\116\033")sleep(100)bot:remove()break end end end
