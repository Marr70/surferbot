ids=N.id,jumlah=N.count})sleep(50)end end function is_included(V,N)for N,c in pairs(N)do if V==c.ids then return true end end return false end function is_changed(V,N,c)for c,g in pairs(c)do if V==g.ids then if N==g.jumlah then return false else return true end end end end function count_changed(V,N)for N,c in pairs(N)do if V==c.ids then return bot:getItemCount(V)-c.jumlah end end end function get_found_item()for V,N in pairs(bot:getInventory())do if N.id==2286 then sleep(50)else if not is_included(N.id,goods)or is_changed(N.id,N.count,goods)then local V=(getItemInfo(N.id)).name local c=count_changed(N.id,goods)or bot:getItemCount(N.id)return string.format("\070\111\117\110\100 \073\116\101\109\058  \037\115 \040\120\037\115\041",V,c)end end end return"\099\097\110\'\116 \100\101\116\101\114\109\105\110\101 \102\111\117\110\100 \105\116\101\109\115"end function wear(V)local N={}N.type=10 N.int_data=V bot:sendPacketRaw(N)end function getPosX()return math.floor((bot:getLocal()).pos.x/32)end function getPosY()return math.floor((bot:getLocal()).pos.y/32)end function rapid_up()local V=midX local N=midY if not bot:isEquipped(2204)then return true end while math.floor((bot:getLocal()).pos.y/32)>=0 do while bot:isEquipped(2204)do if get_signals()==4 then if bot:findPath(getPosX()+1,getPosY())then sleep(move_delay)end if estatus()~=Connected then return end else break end if not bot:isEquipped(2204)then return true end end if bot:findPath(V,N)then sleep(move_delay)end if estatus()~=Connected then return end while bot:isEquipped(2204)do if get_signals()==4 then if bot:findPath(getPosX()-1,getPosY())then sleep(move_delay)end if estatus()~=Connected then return end else break end if not bot:isEquipped(2204)then return true end end if math.floor((bot:getLocal()).pos.y/32)~=0 then N=N-1 if bot:findPath(V,N)then sleep(1500)if get_signals()~=4 then return false end end if estatus()~=Connected then return end else return false end end end function rapid_down()local V=midX local N=mi