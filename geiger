n sleep(move_delay)break end end end if estatus()~=Connected then return false end elseif getPosX()-moveSpeed<0 and getPosY()+moveSpeed>53 then if bot:findPath(0,53)then sleep(move_delay)local S=get_signals()if S>startSignal then midX,midY=getPosX(),getPosY()startSignal=S return true elseif S<startSignal then if bot:findPath(midX,midY)then sleep(move_delay)break end else if bot:findPath(midX,midY)then sleep(move_delay)break end end end if estatus()~=Connected then return false end end end else break end end end while getPosX()~=0 or getPosY()~=0 do if not bot:isEquipped(2204)then startSignal=4 return true else if not bltopleft then if getPosX()-moveSpeed>=0 and getPosY()-moveSpeed>=0 then if bot:findPath(getPosX()-moveSpeed,getPosY()-moveSpeed)then sleep(move_delay)local S=get_signals()if S>startSignal then midX,midY=getPosX(),getPosY()startSignal=S return true elseif S<startSignal then if bot:findPath(midX,midY)then sleep(move_delay)break end end end if estatus()~=Connected then return false end elseif getPosX()-moveSpeed<0 or getPosY()-moveSpeed<0 then if getPosX()-moveSpeed>=0 and getPosY()-moveSpeed<0 then if bot:findPath(getPosX()-moveSpeed,0)then sleep(move_delay)local S=get_signals()if S>startSignal then midX,midY=getPosX(),getPosY()startSignal=S return true elseif S<startSignal then if bot:findPath(midX,midY)then sleep(move_delay)break end else if bot:findPath(midX,midY)then sleep(move_delay)break end end end if estatus()~=Connected then return false end elseif getPosX()-moveSpeed<0 and getPosY()-moveSpeed>=0 then if bot:findPath(0,getPosY()-moveSpeed)then sleep(move_delay)local S=get_signals()if S>startSignal then midX,midY=getPosX(),getPosY()startSignal=S return true elseif S<startSignal then if bot:findPath(midX,midY)then sleep(move_delay)break end else if bot:findPath(midX,midY)then sleep(move_delay)break end end end if estatus()~=Connected then return false end elseif getPosX()-moveSpeed<0 and getPosY()-moveSpeed<0 then if bot:findPath(0,0)then sleep(move_delay)local S=get_signals()if S>startSignal then midX,midY=getPosX(),getPosY()startSignal=S return true elseif S<startSignal then if bot:findPath(midX,midY)then sleep(move_delay)break end else if bot:findPath(midX,midY)then sleep(move_delay)break end end end if estatus()~=Connected then return false end end end else break end end end if bot:findPath(midX,midY)then sleep(move_delay)return false end end function log_msg(S)log(string.format("\091\037\115\093 \037\115 \n",(bot:getLocal()).name,S))if webhook_enabled then send_webhook(S)end end function logs(S)log(string.format("\091\037\115\093 \037\115 \n",(bot:getLocal()).name,S))end function get_enet_status()local S local f=bot:getEnetStatus()if f==Disconnected then S="\068\105\115\099\111\110\110\101\099\116\101\100"elseif f==Connected then S="\067\111\110\110\101\099\116\101\100"elseif f==ShadowBan then S="\083\104\097\100\111\119 \066\097\110"end return S end function reconInfo()ut=os.time()-tt if get_enet_status()=="\068\105\115\099\111\110\110\101\099\116\101\100"then color=9242129 elseif get_enet_status()=="\067\111\110\110\101\099\116\101\100"then color=756998 end local S="        \123\n            \"\099\111\110\116\101\110\116\"\058 \"\064\101\118\101\114\121\111\110\101\"\044\n            \"\101\109\098\101\100\115\"\058 \091\n              \123\n                \"\116\105\116\108\101\"\058 \"\091"..(string.lower((bot:getLocal()).name)..("\093 \115\116\097\116\117\115 \105\115 "..(get_enet_status()..("\"\044\n                \"\099\111\108\111\114\"\058 "..(color..("\044\n                \"\102\111\111\116\101\114\"\058 \123\n                    \"\116\101\120\116\"\058 \"\066\111\116 \085\112\116\105\109\101\058 "..(math.floor(ut/86400)..(" \068\097\121 "..(math.floor((ut%86400)/3600)..(" \072\111\117\114 "..(math.floor(((ut%86400)%3600)/60)..(" \077\105\110\117\116\101 \092\110\077\097\114\114\'\115 \083\099\114\105\112\116 \124 "..(os.date("\033\037\097 \037\098 \037\100\044 \037\089 \097\116 \037\073\058\037\077 \037\112",os.time()+25200).."\"\n                \125\n              \125\n            \093\044\n            \"\117\115\101\114\110\097\109\101\"\058 \"\083\116\097\116\117\115 \076\111\103\115\"\044\n            \"\097\118\097\116\097\114\095\117\114\108\"\058 \"\104\116\116\112\115\058\047\047\105\046\105\098\098\046\099\111\047\077\083\104\070\057\074\081\047\097\118\097\116\097\114-\098\054\056\054\056\048\053\049-\054\050\054\054-\052\048\051\100-\098\057\100\055-\098\102\101\052\054\098\056\057\053\048\051\098\046\106\112\103\"\044\n            \"\097\116\116\097\099\104\109\101\110\116\115\"\058 \091\093\n        \125\n    ")))))))))))))RequestINFO={}RequestINFO.url=webhook_status_url RequestINFO.method=POST RequestINFO.json=true RequestINFO.postData=S x=httpReq(RequestINFO)end function send_webhook(S)ut=os.time()-tt local f="        \123\n            \"\099\111\110\116\101\110\116\"\058 \110\117\108\108\044\n            \"\101\109\098\101\100\115\"\058 \091\n              \123\n                \"\116\105\116\108\101\"\058 \""..(string.lower(((getBot()):getLocal()).name)..(" \091\124\124"..(bot:getCurrentWorld()..("\124\124\093\"\044\n                \"\100\101\115\099\114\105\112\116\105\111\110\"\058 \""..(S..("\"\044\n                \"\099\111\108\111\114\"\058 \049\048\048\053\055\051\053\044\n                \"\102\111\111\116\101\114\"\058 \123\n                  \"\116\101\120\116\"\058 \"\066\111\116 \085\112\116\105\109\101\058 "..(math.floor(ut/86400)..(" \068\097\121 "..(math.floor((ut%86400)/3600)..(" \072\111\117\114 "..(math.floor(((ut%86400)%3600)/60)..(" \077\105\110\117\116\101 \092\110\077\097\114\114\'\115 \083\099\114\105\112\116 \124 "..(os.date("\033\037\097 \037\098 \037\100\044 \037\089 \097\116 \037\073\058\037\077 \037\112",os.time()+25200).."\"\n                \125\n              \125\n            \093\044\n            \"\117\115\101\114\110\097\109\101\"\058 \"\065\117\116\111 \071\101\105\103\101\114 \076\111\103\115\"\044\n            \"\097\118\097\116\097\114\095\117\114\108\"\058 \"\104\116\116\112\115\058\047\047\105\046\105\098\098\046\099\111\047\077\083\104\070\057\074\081\047\097\118\097\116\097\114-\098\054\056\054\056\048\053\049-\054\050\054\054-\052\048\051\100-\098\057\100\055-\098\102\101\052\054\098\056\057\053\048\051\098\046\106\112\103\"\044\n            \"\097\116\116\097\099\104\109\101\110\116\115\"\058 \091\093\n        \125")))))))))))))RequestINFO={}RequestINFO.url=webhook_url RequestINFO.method=POST RequestINFO.json=true RequestINFO.postData=f x=httpReq(RequestINFO)end function tile_drop(S,f,X)local q=0 local F=0 for X,G in pairs(bot:getObjects())do if math.floor(G.pos.x/32)==S and math.floor(G.pos.y/32)==f then q=q+G.count F=F+1 end end if F<20 and q<=4000-X then return true end return false end function drop(S,f)if f==nil then f=bot:getItemCount(S)end bot:sendPacket("\097\099\116\105\111\110\124\100\114\111\112\n\124\105\116\101\109\073\068\124"..S,2)bot:sendPacket("\097\099\116\105\111\110\124\100\105\097\108\111\103\095\114\101\116\117\114\110\n\100\105\097\108\111\103\095\110\097\109\101\124\100\114\111\112\095\105\116\101\109\n\105\116\101\109\073\068\124"..(S..("\124\n\099\111\117\110\116\124"..f)),2)sleep(1000)end function drop_coord()sleep(2000)local S local f for X,q in pairs(drop_list)do if contains(goods,q)then S=1 f="\114\105\103\104\116"else S=-1 f="\108\101\102\116"end sleep(500)while getPosX()~=storage_pos.x or getPosY()~=storage_pos.y do if bot:findPath(storage_pos.x,storage_pos.y)then sleep(1500)end if estatus()~=Connected then return end end while bot:getItemCount(q)>0 do if tile_drop(storage_pos.x+S,getPosY(),bot:getItemCount(q))then face_side(f)log_msg(string.format("\068\114\111\112\112\105\110\103 \082\101\119\097\114\100\058 \037\105\120 \037\115",bot:getItemCount(q),(getItemInfo(q)).name))sleep(1500)while bot:getItemCount(q)>0 and tile_drop(storage_pos.x+S,getPosY(),bot:getItemCount(q))do drop(q)sleep(1500)end else bot:move(UP,1)sleep(2000)if estatus()~=Connected then return end end end end end function drop_blockid()sleep(2000)local S for f,X in pairs(drop_list)do if contains(goods,X)then S=storage_pos.good else S=storage_pos.bad end sleep(500)for f,q in pairs(bot:getTiles())do if q.fg==S or q.bg==S then if tile_drop(q.x,q.y,bot:getItemCount(X))then while getPosX()~=q.x+1 or getPosY()~=q.y do if bot:findPath(q.x+1,q.y)then sleep(1500)end if estatus()~=Connected then return end end face_side("\108\101\102\116")log_msg(string.format("\068\114\111\112\112\105\110\103 \082\101\119\097\114\100\058 \037\105\120 \037\115",bot:getItemCount(X),(getItemInfo(X)).name))sleep(1500)while bot:getItemCount(X)>0 and tile_drop(q.x,q.y,bot:getItemCount(X))do drop(X)sleep(1500)end end end if bot:getItemCount(X)==0 then break end end end end function drop_item()bot:autoCollect(2,false)local S local f if custom_storage then tele(storage_name,storage_id_door)if not use_coord then drop_blockid()if estatus()~=Connected then return end else drop_coord()if estatus()~=Connected then return end end sleep(2000)drop_list={}tele(geiger_world,geiger_id)return end if not custom_storage then tele(geiger_world,geiger_id)for X,q in pairs(drop_list)do if contains(goods,q)then S=1 f="\114\105\103\104\116"else S=-1 f="\108\101\102\116"end sleep(500)while getPosX()~=45 or getPosY()~=53 do if bot:findPath(45,53)then sleep(1500)end if estatus()~=Connected then return end end while bot:getItemCount(q)>0 do if tile_drop(45+S,getPosY(),bot:getItemCount(q))then face_side(f)log_msg(string.format("\068\114\111\112\112\105\110\103 \082\101\119\097\114\100\058 \037\105\120 \037\115",bot:getItemCount(q),(getItemInfo(q)).name))sleep(1500)while bot:getItemCount(q)>0 and tile_drop(45+S,getPosY(),bot:getItemCount(q))do drop(q)sleep(1500)end else bot:move(UP,1)sleep(2000)if estatus()~=Connected then return end end end end drop_list={}end end function check_inventory()for S,f in pairs(items)do if bot:getItemCount(f)>0 then table.insert(drop_list,f)end end end function tele(S,f)while string.upper((bot:getWorld()).name)~=string.upper(S)do bot:sendPacket("\097\099\116\105\111\110\124\106\111\105\110\095\114\101\113\117\101\115\116\n\110\097\109\101\124"..(string.upper(S).."\n\105\110\118\105\116\101\100\087\111\114\108\100\124\048"),3)sleep(warp_delay)end if f==""or f==nil then return end while(bot:getTile(math.floor((bot:getLocal()).pos.x/32),math.floor((bot:getLocal()).pos.y/32))).fg==6 do bot:sendPacket("\097\099\116\105\111\110\124\106\111\105\110\095\114\101\113\117\101\115\116\n\110\097\109\101\124"..(string.upper(S)..("\124"..(string.upper(f).."\n\105\110\118\105\116\101\100\087\111\114\108\100\124\048"))),3)sleep(warp_delay)end end function charge_geiger()if rest_in_maindoor then if bot:findPath((bot:getWorld()).maindoor.x,(bot:getWorld()).maindoor.y)then sleep(1000)end if estatus()~=Connected then reconInfo()connect()reconInfo()end end logs("\067\104\097\114\103\105\110\103 \071\101\105\103\101\114")while bot:getItemCount(2204)==0 and bot:getItemCount(2286)>0 do sleep(60000)if estatus()~=Connected then reconInfo()connect()reconInfo()end if bot:getItemCount(2204)>0 then break end end end function process_auto_geiger()check_inventory()sleep(2000)if#drop_list>0 then sleep(2000)drop_item()return end if charging then charge_geiger()charging=false return end main()sleep(2000)end function contains(S,f)for S,X in pairs(S)do if X==f then return true end end return false end function is_valid()local S={Disconnected;Connected;Update_Required,SERVER_OVERLOADED;OnSendToServer;LogonATTEMPTS;Success}return contains(S,bot:getBotStatus())end function connect()while true do while bot:getEnetStatus()~=Connected do bot:reConnect()sleep(20000)if not is_valid()then while true do sleep(10000)end end end if bstatus()==Success then break else bot:disconnect()sleep(10000)end end end function cari_owner()tele(world_owner)sleep(2000)bot:say("\087\097\105\116\105\110\103 \102\111\114 \096\050\111\119\110\101\114")sleep(5000)while not found_owner do for S,f in pairs(bot:getPlayers())do local X=f.name:match("\096\037\103\040\091\037\119\093\043\041\096\096")if check_owner_table(owner,X)then bot:say("\096\050"..(X.." \096\048\070\111\117\110\100"))bot:say("\083\116\097\114\116\105\110\103 \115\099\114\105\112\116")sleep(5000)found_owner=true end sleep(500)end end end function lolos(S,f)for S,X in pairs(S)do if string.upper((X:getLocal()).name)==string.upper(f)then return true end end return false end function check_duplicate()local S=has_same_worldname(bot_settings)if#S>1 then multibot=true if#S>3 then local f=""for S,X in pairs(S)do f=f..("\091"..(X.."\093  "))end log(f..("\n\084\104\101\114\101 \097\114\101 "..(#S.." \098\111\116 \119\105\116\104 \115\097\109\101 \119\111\114\108\100\095\110\097\109\101\044 \116\104\101 \115\099\114\105\112\116 \119\105\108\108 \110\111\116 \114\117\110 \102\111\114 \116\104\101\115\101 \098\111\116\n")))stop_script=true return end for S,f in pairs(S)do if not lolos(getAllBot(),f)then log("\073\102 \121\111\117 \119\097\110\116 \116\111 \117\115\101 \109\117\108\116\105\112\108\101 \098\111\116 \105\110 \111\110\101 \119\111\114\108\100\044 \112\108\101\097\115\101 \109\097\107\101 \115\117\114\101 \098\111\116 \105\115 \105\110 \111\110\101 \114\100\112\n")stop_script=true return end end end end function start_geiger()geiger_world=bot_settings[string.upper(((getBot()):getLocal()).name)].world_name geiger_id=bot_settings[string.upper(((getBot()):getLocal()).name)].world_id_door if bot_settings[string.upper(((getBot()):getLocal()).name)].storage_name~=""then custom_storage=true storage_name=bot_settings[string.upper(((getBot()):getLocal()).name)].storage_name storage_id_door=bot_settings[string.upper(((getBot()):getLocal()).name)].storage_id_door check_table(storage_pos)end check_duplicate()if stop_script then return end cari_owner()tt=os.time()logs("\083\116\097\114\116\101\100")while is_valid()do if bot:getEnetStatus()==Connected then process_auto_geiger()if stop_script then return end else reconInfo()connect()reconInfo()end end end
