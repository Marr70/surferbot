es,T)elseif bot:findPath(x.x,x.y-1)then sleep(500)punch(0,1)sleep(1000)table.remove(waste_tiles,T)elseif bot:findPath(x.x+1,x.y)then sleep(500)punch(-1,0)sleep(1000)table.remove(waste_tiles,T)elseif bot:findPath(x.x-1,x.y)then sleep(500)punch(1,0)sleep(1000)table.remove(waste_tiles,T)end end end logs("\068\111\110\101 \067\108\101\097\114\105\110\103 \084\111\120\105\099 \087\097\115\116\101")end end function bstatus()return bot:getBotStatus()end function estatus()return bot:getEnetStatus()end function cek_multibot(T,x)for T,f in pairs(T)do if string.upper(f.name)==string.upper(x)then return true end end return false end function main()midX,midY=50,27 goods={}check_geiger()if stop_script then return end if charging then return end tele(geiger_world,geiger_id)if multibot then for T,x in pairs(duplicate_line)do if not(getBot(x)):isEquipped(2204)then while(getBot(x)):getItemCount(2204)==0 and(getBot(x)):getItemCount(2286)==0 do sleep(2000)end end end for T,x in pairs(duplicate_line)do if string.upper(x)==string.upper(((getBot()):getLocal()).name)then break end if string.upper(x)~=string.upper(((getBot()):getLocal()).name)then while(getBot(x)):getItemCount(2286)==0 do sleep(2000)end end end end sleep(2000)break_toxic_waste()reserve_items()blup=false bldown=false blright=false blleft=false bltopright=false blbottomright=false blbottomleft=false bltopleft=false if bot:findPath(midX,midY)then sleep(2000)startSignal=get_signals()end if estatus()~=Connected then return end t=os.time()while true do sleep(1000)if startSignal==4 then if estatus()~=Connected then return end sleep(1000)rapid_green()sleep(1000)if estatus()~=Connected then return end if not bot:isEquipped(2204)then break end else while true do if((os.time()-t)%3600)/60>10 then break end if checkDirection()then break end if estatus()~=Connected then return end if((os.time()-t)%3600)/60>10 then break end if checkDiagonal()then break end if estatus()~=Connected then return end if((os.time()-t)%3600)/60>10 then break e