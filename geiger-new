x()]=S end end return I end function punch(S,I)local K=bot:getLocal()return bot:hitTile(math.floor(K.pos.x/32)+S,math.floor(K.pos.y/32)+I)end function break_toxic_waste()waste_tiles={}for S,I in ipairs(bot:getTiles())do if I.fg==778 or I.bg==778 then table.insert(waste_tiles,{x=I.x;y=I.y})end end if#waste_tiles>0 then logs("\067\108\101\097\114\105\110\103 \084\111\120\105\099 \087\097\115\116\101")while#waste_tiles>0 do for S,I in pairs(waste_tiles)do if bot:findPath(I.x,I.y+1)then sleep(500)punch(0,-1)sleep(1000)table.remove(waste_tiles,S)elseif bot:findPath(I.x,I.y-1)then sleep(500)punch(0,1)sleep(1000)table.remove(waste_tiles,S)elseif bot:findPath(I.x+1,I.y)then sleep(500)punch(-1,0)sleep(1000)table.remove(waste_tiles,S)elseif bot:findPath(I.x-1,I.y)then sleep(500)punch(1,0)sleep(1000)table.remove(waste_tiles,S)end end end logs("\068\111\110\101 \067\108\101\097\114\105\110\103 \084\111\120\105\099 \087\097\115\116\101")end end function bstatus()return bot:getBotStatus()end function estatus()return bot:getEnetStatus()end function cek_multibot(S,I)for S,K in pairs(S)do if string.upper(K.name)==string.upper(I)then return true end end return false end function main()midX,midY=50,27 goods={}check_geiger()if stop_script then return end if charging then return end tele(geiger_world,geiger_id)if multibot then for S,I in pairs(duplicate_line)do if not(getBot(I)):isEquipped(2204)then while(getBot(I)):getItemCount(2204)==0 and(getBot(I)):getItemCount(2286)==0 do sleep(2000)end end end for S,I in pairs(duplicate_line)do if string.upper(I)==string.upper(((getBot()):getLocal()).name)then break end if string.upper(I)~=string.upper(((getBot()):getLocal()).name)then while(getBot(I)):getItemCount(2286)==0 do sleep(2000)if(getBot(I)):getEnetStatus()~=Connected and(getBot(I)):getBotStatus()~=Success then break end end end end end sleep(2000)break_toxic_waste()reserve_items()blup=false bldown=false blright=false blleft=false bltopright=false blbottomright=false blbottomleft=false b