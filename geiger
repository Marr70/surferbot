bot=getBot()items={6416;3196,1500,1498;2806,2804;8270;8272;8274,4676,4678,4680,4682,4652,4650,4648,4646,11186;10086;10084;2206,2244;2246,2242;2248;2250;3792;3306,4654,3204,12502}goods={2206,2242,2244;2246;2248;2250,3792;3306,4654}drop_list={}duplicate_line={}stop_script=false custom_storage=false use_coord=false charging=false found_owner=false multibot=false function check_table(v)if v.x and v.y then use_coord=true elseif v.good and v.bad then use_coord=false else log("\115\116\111\114\097\103\101\095\112\111\115 \110\111\116 \118\097\108\105\100\044 \099\104\101\099\107 \101\120\097\109\112\108\101 \105\110 \115\101\114\118\101\114\n")stop_script=true return end end function check_owner_table(v,l)for v,M in pairs(v)do if string.upper(M)==string.upper(l)then return true end end return false end function get_signals()sleep(500)local v=bot:getSignal()sleep(500)if v==RAPIDGREEN then return 4 elseif v==GREEN then return 3 elseif v==YELLOW then return 2 elseif v==RED then return 1 else return 0 end end function round(v)return v%1>.5 and math.ceil(v)or math.floor(v)end function collect(v,l)for M,a in pairs(bot:getObjects())do local w=math.floor(a.pos.x/32)local O=math.floor(a.pos.y/32)if bot:isInside(w,O,v)then if l==nil or a.id==l then bot:collectObject(a.uid)end end end end function face_side(v)local l={}l.type=0 if v=="\108\101\102\116"then l.flags=48 elseif v=="\114\105\103\104\116"then l.flags=32 end l.pos_x=(bot:getLocal()).pos.x l.pos_y=(bot:getLocal()).pos.y bot:sendPacketRaw(l)end function take()sleep(1000)bot:autoCollect(2,false)sleep(200)tele(world_take_geiger,id_door_take_geiger)sleep(200)while bot:getItemCount(2204)==0 do while true do for v,l in pairs(bot:getObjects())do if l.id==2204 then if bot:findPath(round(l.pos.x/32)-1,math.floor(l.pos.y/32))then sleep(1000)collect(2)sleep(1000)end end end if bot:getItemCount(2204)>0 then break end end end sleep(2000)while not bot:isEquipped(2204)do wear(2204)sleep(3000)end if bot:getItemCount(2204)>1 then bot:autoCol